  <ontology>
    <term_definitions language="es">
      <items code="at0000">
        <items id="text">Intraocular injection</items>
        <items id="description">Gestiona la planificación y posterior realización de un tratamiento oftalmológico que incluya la inyección intraocular.</items>
      </items>
      <items code="at0001">
        <items id="text">Solicitud de medicación</items>
        <items id="description">Detalles de un medicamento, vacuna o cualquier otro elemento terapéutico acompañado de instrucciones para su uso.</items>
      </items>
      <items code="at0002">
        <items id="text">Medicación realizada</items>
        <items id="description">Acción derivada de una orden de medicación creada por un médico que especifique qué medicamentos tomar, cuándo, por cuánto tiempo, etc.</items>
      </items>
      <items code="at0003">
        <items id="text">Reacción adversa</items>
        <items id="description">Cualquier reacción adversa identificada como consecuencia del procedimiento de inyección intravítrea.</items>
      </items>
    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Intraocular injection</items>
        <items id="description">Manages the process of planning and then carrying out an ophthalmic treatment by means of intraocular injection.</items>
      </items>
      <items code="at0001">
        <items id="text">Intravitreal injection order</items>
        <items id="description">Details about the prescription of intravitreal injections.</items>
      </items>
      <items code="at0002">
        <items id="text">Intravitreal injection procedure</items>
        <items id="description">Details of any administration of intravitreal injection conducted.</items>
      </items>
      <items code="at0003">
        <items id="text">Adverse reaction</items>
        <items id="description">Any adverse reaction identified as a consequence of intravitreal injection procedure.</items>
      </items>
    </term_definitions>
  </ontology>
