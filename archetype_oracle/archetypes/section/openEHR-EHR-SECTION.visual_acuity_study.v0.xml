  <ontology>
    <term_definitions language="es">
      <items code="at0000">
        <items id="text">Estudio de la agudeza visual</items>
        <items id="description">Define el proceso que implica la prueba y el posterior estudio de los resultados de agudeza visual.</items>
      </items>
      <items code="at0001">
        <items id="text">Procedimiento realizado</items>
        <items id="description">Gestiona la realización de las actividades destinadas a obtener el valor de la agudeza visual del paciente.</items>
      </items>
      <items code="at0002">
        <items id="text">Prueba de agudeza visual</items>
        <items id="description">Estudio de los resultados de la prueba de agudeza visual.</items>
      </items>
    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Visual acuity study</items>
        <items id="description">Defines the process which involves the visual acuity test and the subsequent study of results.</items>
      </items>
      <items code="at0001">
        <items id="text">Procedure undertaken</items>
        <items id="description">Manages the realization of the activities aimed at obtain the visual acuity value from the patient.</items>
      </items>
      <items code="at0002">
        <items id="text">Visual acuity test</items>
        <items id="description">Examination of results for the visual acuity test.</items>
      </items>
    </term_definitions>
  </ontology>
