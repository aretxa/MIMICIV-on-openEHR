  <ontology>
    <term_definitions language="es">
      <items code="at0000">
        <items id="text">Estudio de presión intraocular</items>
        <items id="description">Define el proceso que implica la adquisición y estudio de la presión intraocular.</items>
      </items>
      <items code="at0001">
        <items id="text">Procedimiento realizado</items>
        <items id="description">Actividad clínica que se ha llevado a cabo con propósitos terapéuticos o diagnósticos.</items>
      </items>
      <items code="at0002">
        <items id="text">Medición de PIO</items>
        <items id="description">Medición de la presión intraocular de un solo ojo, normalmente utilizando un dispositivo de tonometría.</items>
      </items>
    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Intraocular pressure study</items>
        <items id="description">Defines the process which involves the intraocular pressure acquisition and its study.</items>
      </items>
      <items code="at0001">
        <items id="text">Procedure undertaken</items>
        <items id="description">Describes the procedure/s carried out to obtain the intraocular pressure from the patient.</items>
      </items>
      <items code="at0002">
        <items id="text">IOP measurement</items>
        <items id="description">Examination of results for the intraocular pressure measurement of a single eye, normally using a tonometry device.</items>
      </items>
    </term_definitions>
  </ontology>
