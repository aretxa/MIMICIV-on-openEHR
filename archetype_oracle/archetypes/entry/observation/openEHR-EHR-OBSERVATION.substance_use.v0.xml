  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Substance use</items>
        <items id="description">The consumption of a specific substance or class of substances that may harm an individual's health or social well-being at a specific point or interval of time.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
