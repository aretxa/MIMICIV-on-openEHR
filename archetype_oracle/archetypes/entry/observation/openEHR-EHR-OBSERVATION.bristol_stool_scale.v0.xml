    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Bristol stool scale</items>
        <items id="description">Classification of faeces characteristics according to the Bristol Stool Scale/Score.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Point-in-time</items>
        <items id="description">Default, unspecified point in time event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the stool scale, not captured in other fields.</items>
      </items>
      <items code="at0005">
        <items id="text">Bristol stool scale</items>
        <items id="description">A score to assess the characteristics of faeces after defaecation.</items>
      </items>
      <items code="at0006">
        <items id="text">Separate hard lumps, like nuts (hard to pass)</items>
        <items id="description">Stool consists of separate hard lumps, like nuts (hard to pass).</items>
      </items>
      <items code="at0007">
        <items id="text">Sausage-shaped, but lumpy</items>
        <items id="description">Stool is sausage-shaped, but lumpy.</items>
      </items>
      <items code="at0008">
        <items id="text">Like a sausage but with cracks on its surface</items>
        <items id="description">Stool is like a sausage but with cracks on its surface.</items>
      </items>
      <items code="at0009">
        <items id="text">Like a sausage or snake, smooth and soft</items>
        <items id="description">Stool is like a sausage or snake, smooth and soft.</items>
      </items>
      <items code="at0010">
        <items id="text">Soft blobs with clear cut edges (passed easily)</items>
        <items id="description">Stool consists of soft blobs with clear cut edges (passed easily).</items>
      </items>
      <items code="at0011">
        <items id="text">Fluffy pieces with ragged edges, a mushy stool</items>
        <items id="description">Stool consists of fluffy pieces with ragged edges, a mushy stool.</items>
      </items>
      <items code="at0012">
        <items id="text">Watery, no solid pieces. Entirely liquid</items>
        <items id="description">Stool is watery, no solid pieces. Entirely liquid.</items>
      </items>
      <items code="at0013">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0014">
        <items id="text">Confounding factors</items>
        <items id="description">Record any issues or factors that may impact on the stool scale.</items>
      </items>
      <items code="at0015">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0016">
        <items id="text">Extension</items>
        <items id="description">Additional information required to capture local context or to align with other reference models/formalisms.</items>
        <items id="comment">For example: Local hospital departmental infomation or additional metadata to align with FHIR or CIMI equivalents.</items>
      </items>
    </term_definitions>
