  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Tobacco Use</items>
        <items id="description">Details about use of all forms of tobacco and tobacco-containing products at a specified point in time or over a specified period of time.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">An unspecified event.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Tobacco Use?</items>
        <items id="description">Is any tobacco being used or consumed?</items>
        <items id="comment">Record as True if the individual is using tobacco; Record as False if not using tobacco.</items>
      </items>
      <items code="at0005">
        <items id="text">Usage Details</items>
        <items id="description">Details about the use or consumption of specific forms of tobacco.</items>
      </items>
      <items code="at0006">
        <items id="text">Form</items>
        <items id="description">Form of tobacco used or consumed.</items>
      </items>
      <items code="at0007">
        <items id="text">Cigarette</items>
        <items id="description">Manufactured cigarettes, containing processed tobacco and rolled into a cylinder.</items>
      </items>
      <items code="at0008">
        <items id="text">Roll-Your-Own</items>
        <items id="description">Hand-filled cigarettes using loose tobacco rolled in a cigarette paper.</items>
      </items>
      <items code="at0009">
        <items id="text">Cigar</items>
        <items id="description">Air-cured and fermented tobaccos with a tobacco-leaf wrapper.</items>
      </items>
      <items code="at0010">
        <items id="text">Pipe</items>
        <items id="description">Tobacco placed in a bowl and smoke inhaled through a stem.</items>
      </items>
      <items code="at0012">
        <items id="text">Waterpipe</items>
        <items id="description">Tobacco, often flavoured, is burned then cooled through a basin of water eand consumed through a hose and mouthpiece.</items>
      </items>
      <items code="at0013">
        <items id="text">Smokeless tobacco</items>
        <items id="description">Tobacco consumed by inhalation or ingestion - including snuff, snus, gutka, dipping tobacco and dissolvable tobacco.</items>
      </items>
      <items code="at0014">
        <items id="text">Number Smoked</items>
        <items id="description">Number of units containing tobacco consumed.</items>
        <items id="comment">For example, eg cigarettes or cigars.</items>
      </items>
      <items code="at0015">
        <items id="text">Amount</items>
        <items id="description">Amount of tobacco used or consumed.</items>
        <items id="comment">Only one option allowed - either number smoked or weight consumed - per tobacco form.</items>
      </items>
      <items code="at0016">
        <items id="text">Weight Consumed</items>
        <items id="description">Weight of tobacco consumed.</items>
        <items id="comment">For example, smoked in a pipe or amount of chewing tobacco.</items>
      </items>
      <items code="at0017">
        <items id="text">Triggers</items>
        <items id="description">Identified triggers which stimulate or cause use of this form of tobacco.</items>
      </items>
      <items code="at0018">
        <items id="text">Context</items>
        <items id="description">Context of use of this form of tobacco.</items>
        <items id="comment">For example, when in a social situation, driving a car or talking on the telephone.</items>
      </items>
      <items code="at0019">
        <items id="text">Evidence of Dependence</items>
        <items id="description">Description of any evidence of dependence on tobacco.</items>
      </items>
      <items code="at0021">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the tobacco use or consumption not captured in other fields.</items>
      </items>
      <items code="at0022">
        <items id="text">Average Use Interval</items>
        <items id="description">Average, or typical, typical use over a specified time interval. For example, allows recording of average number of cigarettes smoked per week for the previous 10 years.</items>
      </items>
      <items code="at0023">
        <items id="text">Specified Day</items>
        <items id="description">Actual tobacco consumption on a specified day. Supports recording consumption in a Smoking Diary.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

