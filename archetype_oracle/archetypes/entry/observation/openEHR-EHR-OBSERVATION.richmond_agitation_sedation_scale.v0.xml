  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Richmond agitation sedation scale (RASS)</items>
        <items id="description">A scale used to measure the agitation or sedation level of a patient.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">*</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Scale</items>
        <items id="description">The Richmond agitation/sedation scale.</items>
      </items>
      <items code="at0005">
        <items id="text">Combative</items>
        <items id="description">	Overtly combative or violent; immediate danger to staff.</items>
      </items>
      <items code="at0006">
        <items id="text">Very agitated</items>
        <items id="description">Pulls on or removes tube(s) or catheter(s) or has aggressive behavior toward staff.</items>
      </items>
      <items code="at0007">
        <items id="text">Agitated</items>
        <items id="description">Frequent nonpurposeful movement or patient–ventilator dyssynchrony.</items>
      </items>
      <items code="at0008">
        <items id="text">Restless</items>
        <items id="description">	Anxious or apprehensive but movements not aggressive or vigorous.</items>
      </items>
      <items code="at0009">
        <items id="text">Alert and calm</items>
        <items id="description">Spontaneously pays attention to caregiver</items>
      </items>
      <items code="at0010">
        <items id="text">Drowsy</items>
        <items id="description">Patient has eye opening and eye contact, which is sustained for more than 10 seconds.</items>
      </items>
      <items code="at0011">
        <items id="text">Light sedation</items>
        <items id="description">Patient has eye opening and eye contact, but this is not sustained for 10 seconds.</items>
      </items>
      <items code="at0012">
        <items id="text">Moderate sedation</items>
        <items id="description">Patient has any movement in response to voice, excluding eye contact.</items>
      </items>
      <items code="at0013">
        <items id="text">Deep sedation</items>
        <items id="description">Patient has any movement to physical stimulation.</items>
      </items>
      <items code="at0014">
        <items id="text">Unarousable</items>
        <items id="description">Patient has no response to voice or physical stimulation.</items>
      </items>
    </term_definitions>
    <term_definitions language="es-ar">
      <items code="at0000">
        <items id="text">Escala de Agitación/Sedación de Richmond (RASS)</items>
        <items id="description">Escala utilizada para medir el grado de agitación o sedación de un paciente hospitalizado.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">*</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Escala</items>
        <items id="description">La escala de agitación/sedación de Richmond.</items>
      </items>
      <items code="at0005">
        <items id="text">Combativo</items>
        <items id="description">Abiertamente combativo o violento; peligro inmediato para el personal.</items>
      </items>
      <items code="at0006">
        <items id="text">Muy agitado</items>
        <items id="description">Agresivo, Tira de o se quita tubos o catéteres, o exhibe compotamiento violento hacia el personal.</items>
      </items>
      <items code="at0007">
        <items id="text">Agitado</items>
        <items id="description">Movimientos frecuentes y sin propósito, o lucha con el respirador.</items>
      </items>
      <items code="at0008">
        <items id="text">Inquieto</items>
        <items id="description">Ansioso, pero sin movimientos agresivos o vigorosos.</items>
      </items>
      <items code="at0009">
        <items id="text">Alerta y calmado</items>
        <items id="description">Presta atención al cuidador en forma espontanea.</items>
      </items>
      <items code="at0010">
        <items id="text">Somnoliento</items>
        <items id="description">El paciente tiene apertura ocular, que sostiene por mas de 10 segundos.</items>
      </items>
      <items code="at0011">
        <items id="text">Sedación leve</items>
        <items id="description">El paciente tiene apertura ocular y hace contacto visual, pero no lo sostiene sostiene por mas de 10 segundos.</items>
      </items>
      <items code="at0012">
        <items id="text">Sedación moderada</items>
        <items id="description">El paciente tiene movimientos en respuesta a estímulos verbales pero no realiza contacto visual.</items>
      </items>
      <items code="at0013">
        <items id="text">Sedación profunda</items>
        <items id="description">El paciente tiene movimientos en respuesta a estímulos físicos.</items>
      </items>
      <items code="at0014">
        <items id="text">Sin respuesta</items>
        <items id="description">El paciente no tiene movimientos en respuesta a estímulos verbales o físicos.</items>
      </items>
    </term_definitions>
  </ontology>
