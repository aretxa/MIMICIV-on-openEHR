  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Telecommunication Record</items>
        <items id="description">Record of a telecommunication, usually a text conversation, or video- or teleconference recording between two or more parties, captured as part of the record of health care.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Start Time</items>
        <items id="description">The time that represents the start of the telecommunication.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the telecommunication.</items>
      </items>
      <items code="at0005">
        <items id="text">Record</items>
        <items id="description">The actual record of any telecommunication between two or more healthcare providers.</items>
      </items>
      <items code="at0006">
        <items id="text">Recording Interval</items>
        <items id="description">The period of time over which the telecommunication was recorded.</items>
      </items>
      <items code="at0008">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0009">
        <items id="text">Device</items>
        <items id="description">Details of device used to capture the telecommunication.</items>
      </items>
      <items code="at0010">
        <items id="text">Extension</items>
        <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
        <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
      </items>
    </term_definitions>
  </ontology>
