  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Capillary refill time (CRT)</items>
        <items id="description">Measurement of the time taken for color to return to an external capillary bed after pressure is applied to cause blanching.</items>
        <items id="comment"> Sometimes expressed as 'capillary return'.</items>
      </items>
      <items code="at0001">
        <items id="text">History</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Clinical interpretation</items>
        <items id="description">A clinical interpretation of the capillary refill time.</items>
        <items id="comment">Normal range may vary based on age.</items>
      </items>
      <items code="at0006">
        <items id="text">Normal capillary filling time</items>
        <items id="description"></items>
      </items>
      <items code="at0007">
        <items id="text">Increased capillary filling time</items>
        <items id="description"></items>
      </items>
      <items code="at0008">
        <items id="text">Decreased capillary filing time</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">Refill time</items>
        <items id="description">The measured time taken for color to return after blanching.</items>
      </items>
      <items code="at0019">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0020">
        <items id="text">Extension</items>
        <items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
        <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
      </items>
      <items code="at0021">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0023">
        <items id="text">Confounding factors</items>
        <items id="description">Issues or factors that may impact on measurement of the capillary refill time, not captured in other fields.</items>
        <items id="comment">For example: pain; fever; ambient temperature and light; use of vasoactive medication or peripheral vascular occlusion.</items>
      </items>
      <items code="at0024">
        <items id="text">Location of measurement</items>
        <items id="description">Simple body site where the capillary filling time was measured.</items>
        <items id="comment">For example: 'nail bed right index finger', 'forehead', 'sternum' and 'skin patch left breast'.</items>
      </items>
      <items code="at0037">
        <items id="text">Device</items>
        <items id="description">Details about any device used to measure capillary refill time.</items>
      </items>
      <items code="at0039">
        <items id="text">Structured measurement location</items>
        <items id="description">Structured anatomical location of where the measurement was taken.</items>
      </items>
      <items code="at0040">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the capillary refill time not captured in other fields.</items>
      </items>
      <items code="at0041">
        <items id="text">Measurement method</items>
        <items id="description">Narrative description of the method used for measuring the capillary refill time.</items>
        <items id="comment">Specify in order to ensure that a sequence of measurements are taken in the same way.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Kapillær fylningstid (CRT)</items>
        <items id="description">Måling av tiden det tar før fargen vender tilbake i en ekstern kapillærseng etter å ha blitt utsatt for ytre press.</items>
        <items id="comment">Noen ganger benevnt som "kapillær refill".</items>
      </items>
      <items code="at0001">
        <items id="text">History</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Uspesifisert hendelse</items>
        <items id="description">Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i et templat eller i en applikasjon.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Klinisk tolkning</items>
        <items id="description">En klinisk tolkning av tiden for kapillær refylning.</items>
        <items id="comment">Normalområdet kan variere med alder.</items>
      </items>
      <items code="at0006">
        <items id="text">Normal kapillær fylningstid</items>
        <items id="description">*(en)</items>
      </items>
      <items code="at0007">
        <items id="text">Økt kapillær fylningstid</items>
        <items id="description">*(en)</items>
      </items>
      <items code="at0008">
        <items id="text">Redusert kapillær fylningstid</items>
        <items id="description">*(en)</items>
      </items>
      <items code="at0009">
        <items id="text">Fylningstid</items>
        <items id="description">Den målte tiden det tar før fargen vender tilbake etter å ha blitt utsatt for ytre press.</items>
      </items>
      <items code="at0019">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0020">
        <items id="text">Tilleggsinformasjon</items>
        <items id="description">Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.</items>
        <items id="comment">For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.</items>
      </items>
      <items code="at0021">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0023">
        <items id="text">Konfunderende faktorer</items>
        <items id="description">Fritekstbeskrivelse av problemer eller faktorer som kan ha påvirkning på målingen av kapillær fylningstid.</items>
        <items id="comment">For eksempel smerter, feber, temperatur eller lys i omgivelsene, bruk av vasoaktive medikamenter eller tilstoppet eller hindret perifer sirkulasjon.</items>
      </items>
      <items code="at0024">
        <items id="text">Målested</items>
        <items id="description">Anatomisk sted hvor den kapillære fylningstiden var målt.</items>
        <items id="comment">For eksempel "negleseng 2. høyre finger", "panne", "sternum" eller "fri hudlapp venstre bryst".</items>
      </items>
      <items code="at0037">
        <items id="text">Måleapparat</items>
        <items id="description">Detaljer om eventuelt måleapparat som er brukt til å måle kapillær fylningstid.</items>
      </items>
      <items code="at0039">
        <items id="text">Strukturert målested</items>
        <items id="description">Strukturert anatomisk lokalisering av stedet målingen ble gjort.</items>
      </items>
      <items code="at0040">
        <items id="text">Kommentar</items>
        <items id="description">Ytterligere fritekst om kapillær fylningstid, som ikke er fanget i andre felt.</items>
      </items>
      <items code="at0041">
        <items id="text">Målemetode</items>
        <items id="description">Fritekstbeskrivelse av metoden som ble benyttet for måling av kapillær fylningstid.</items>
        <items id="comment">Spesifiser for å sikre at en sekvens av målinger blir gjort på samme måte.</items>
      </items>
    </term_definitions>
