      <purpose>To record details about faecal output.</purpose>
      <keywords>fluid</keywords>
      <keywords>balance</keywords>
      <keywords>output</keywords>
      <keywords>urine</keywords>
      <keywords>vomit</keywords>
      <keywords>drain</keywords>
