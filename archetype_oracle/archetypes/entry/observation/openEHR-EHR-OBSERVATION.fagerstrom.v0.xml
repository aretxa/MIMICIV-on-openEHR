  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Fagerström test for nicotine dependence</items>
        <items id="description">Standard instrument for assessing physical dependence on nicotine.</items>
      </items>
      <items code="at0001">
        <items id="text">Event Series</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Morning cigarette</items>
        <items id="description">How soon after you wake up do you smoke your first cigarette?</items>
      </items>
      <items code="at0005">
        <items id="text">After 60 Minutes</items>
        <items id="description">First cigarette is smoked more than 60 minutes after waking.</items>
      </items>
      <items code="at0006">
        <items id="text">Within 6-30 Minutes</items>
        <items id="description">First cigarette is smoked between 6 and 30 minutes after waking.</items>
      </items>
      <items code="at0007">
        <items id="text">Within 31-60 Minutes</items>
        <items id="description">First cigarette is smoked between 31 and 60 minutes after waking.</items>
      </items>
      <items code="at0008">
        <items id="text">Within 5 Minutes</items>
        <items id="description">First cigarette is smoked in the first 5 minutes after waking.</items>
      </items>
      <items code="at0009">
        <items id="text">Difficult to refrain</items>
        <items id="description">Do you find it difficult to refrain from smoking in places where it is forbidden (e.g., in church, at the library, in cinema, etc)?</items>
      </items>
      <items code="at0010">
        <items id="text">No</items>
        <items id="description">It is not difficult to refrain from smoking.</items>
      </items>
      <items code="at0011">
        <items id="text">Yes</items>
        <items id="description">It is difficult to refrain from smoking.</items>
      </items>
      <items code="at0012">
        <items id="text">Hate to give up</items>
        <items id="description">Which cigarette would you hate most to give up?</items>
      </items>
      <items code="at0013">
        <items id="text">Any other</items>
        <items id="description">Any cigarette other than the first cigarette in the morning would be most difficult to give up.</items>
      </items>
      <items code="at0014">
        <items id="text">The first in the morning</items>
        <items id="description">First cigarette in the morning would be most difficult to give up.</items>
      </items>
      <items code="at0015">
        <items id="text">Daily consumption</items>
        <items id="description">How many cigarettes per day do you smoke?</items>
      </items>
      <items code="at0016">
        <items id="text">10 or Less</items>
        <items id="description">Smoking &lt;=10 cigarettes per day.</items>
      </items>
      <items code="at0017">
        <items id="text">11-20</items>
        <items id="description">Smoking 11-20 cigarettes per day.</items>
      </items>
      <items code="at0018">
        <items id="text">21-30</items>
        <items id="description">Smoking 21-30 cigarettes per day.</items>
      </items>
      <items code="at0019">
        <items id="text">31 or more</items>
        <items id="description">Smoking &gt;30 cigarettes per day.</items>
      </items>
      <items code="at0020">
        <items id="text">Early morning pattern</items>
        <items id="description">Do you smoke more during the first hours after waking than during the rest of the day?</items>
      </items>
      <items code="at0021">
        <items id="text">No</items>
        <items id="description">Not smoking more during the first hours after waking.</items>
      </items>
      <items code="at0022">
        <items id="text">Yes</items>
        <items id="description">Smoke more during the first hours after waking.</items>
      </items>
      <items code="at0023">
        <items id="text">During illness</items>
        <items id="description">Do you smoke even when you are ill enough to be in bed most of the day?</items>
      </items>
      <items code="at0024">
        <items id="text">No</items>
        <items id="description">Not smoking if ill enough to be in bed most of the day.</items>
      </items>
      <items code="at0025">
        <items id="text">Yes</items>
        <items id="description">Smoke continues, even if ill enough to be in bed most of the day.</items>
      </items>
      <items code="at0026">
        <items id="text">Total</items>
        <items id="description">Aggregate total from all components.</items>
      </items>
      <items code="at0027">
        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
