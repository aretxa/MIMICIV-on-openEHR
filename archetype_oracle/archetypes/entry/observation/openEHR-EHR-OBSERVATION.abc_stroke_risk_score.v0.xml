        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Prior stroke or transient ischaemic attack</items>
        <items id="description">Points assigned based on the presence or absence of a prior history of stroke or TIA in an individual.</items>
      </items>
      <items code="at0006">
        <items id="text">Age</items>
        <items id="description">Points assigned based on an individual's age.</items>
      </items>
      <items code="at0007">
        <items id="text">cTnT-hs</items>
        <items id="description">Points assigned based on an individual's plasma concentration of cardiac troponin-T high-sensitivity (or cardiac troponin-I high sensitivity).</items>
      </items>
      <items code="at0008">
        <items id="text">NT-proBNP</items>
        <items id="description">Points assigned based on an individual's plasma concentration of N-terminal fragment B-type natriuretic peptide.</items>
      </items>
      <items code="at0009">
        <items id="text">Total score</items>
        <items id="description">Sum of the points assigned for each of the contributing variables.</items>
      </items>
      <items code="at0012">
        <items id="text">Comment</items>
