  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Bishop score</items>
        <items id="description">Assessment of readiness of the cervix in anticipation of induction of labour.</items>
      </items>
      <items code="at0001">
        <items id="text">History</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0003">
        <items id="text">ItemTree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Fetal station</items>
        <items id="description">The position of the fetal head in relation to the ischial spines.</items>
        <items id="comment">Negative numbers indicate that the head is further inside than the ischial spines and positive numbers show that the head is below the level of the ischial spines.</items>
      </items>
      <items code="at0005">
        <items id="text">- 3</items>
        <items id="description">Fetal station is 3cm above the level of the ischial spines.</items>
      </items>
      <items code="at0006">
        <items id="text">- 2</items>
        <items id="description">Fetal station is 2cm above the level of the ischial spines.</items>
      </items>
      <items code="at0007">
        <items id="text">-1, 0</items>
        <items id="description">Fetal station is 1cm above the level of the ischial spines or leveled with the ischial spines; 0cm.</items>
      </items>
      <items code="at0008">
        <items id="text">+1, +2</items>
        <items id="description">Fetal station is 1 or 2cm below the level of the ischial spines.</items>
      </items>
      <items code="at0009">
        <items id="text">Dilation</items>
        <items id="description">Diameter of the cervical os.</items>
      </items>
      <items code="at0010">
        <items id="text">Closed</items>
        <items id="description">The cervical os is closed.</items>
      </items>
      <items code="at0011">
        <items id="text">1-2 cm</items>
        <items id="description">The cervical os is 1-2 cm dilated.</items>
      </items>
      <items code="at0012">
        <items id="text">3-4 cm</items>
        <items id="description">The cervical os is 3-4 cm dilated.</items>
      </items>
      <items code="at0013">
        <items id="text">5+ cm</items>
        <items id="description">The cervical os is 5+ cm dilated.</items>
      </items>
      <items code="at0014">
        <items id="text">Effacement</items>
        <items id="description">Thinning of the cervix.</items>
        <items id="comment">A fully effaced cervix (80+%) is described as paper-thin.</items>
      </items>
      <items code="at0015">
        <items id="text">0 - 30%</items>
        <items id="description">Cervical effacement is estimated to 0 - 30%.</items>
      </items>
      <items code="at0016">
        <items id="text">40 - 50%</items>
        <items id="description">Cervical effacement is estimated to 40 - 50%.</items>
      </items>
      <items code="at0017">
        <items id="text">60 - 70%</items>
        <items id="description">Cervical effacement is estimated to 60 - 70%.</items>
      </items>
      <items code="at0018">
        <items id="text">80+ %</items>
        <items id="description">Cervical effacement is estimated to 80+ %.</items>
      </items>
      <items code="at0019">
        <items id="text">Consistency</items>
        <items id="description">The amount of softening of the cervix, indicating a decline in the tissue tensile strength.</items>
      </items>
      <items code="at0020">
        <items id="text">Firm</items>
        <items id="description">The cervix feels firm.</items>
      </items>
      <items code="at0021">
        <items id="text">Medium</items>
        <items id="description">The cervix feels neither firm nor soft, but in between.</items>
      </items>
      <items code="at0022">
        <items id="text">Soft</items>
        <items id="description">The cervix feels soft.</items>
      </items>
      <items code="at0023">
        <items id="text">Position</items>
        <items id="description">Position of the cervix.</items>
      </items>
      <items code="at0024">
        <items id="text">Posterior</items>
        <items id="description">The cervix is located posteriorly, towards the sacrum.</items>
      </items>
      <items code="at0025">
        <items id="text">Middle</items>
        <items id="description">The cervix is located centrally, in the typical anatomical position.</items>
      </items>
      <items code="at0026">
        <items id="text">Anterior</items>
        <items id="description">The cervix is located anteriorly, towards the pubis.</items>
      </items>
      <items code="at0027">
        <items id="text">Total score</items>
        <items id="description">Sum of the individual scores assigned for each of the contributing variables.</items>
        <items id="comment">Total score may be generated at run-time. </items>
      </items>
      <items code="at0028">
        <items id="text">ItemTree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0030">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the overall Bishop score not captured in other fields.</items>
      </items>
      <items code="at0031">
        <items id="text">Extension</items>
        <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
        <items id="comment">For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.</items>
      </items>
    </term_definitions>
