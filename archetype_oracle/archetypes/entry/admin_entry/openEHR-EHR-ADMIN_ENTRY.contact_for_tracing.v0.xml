  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Exposure contact for tracing</items>
        <items id="description">An individual who may be the source of infectious disease and at risk of spreading it to others or an individual who may be at risk after exposure to an infected individual.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0003">
        <items id="text">Contact demographics</items>
        <items id="description"></items>
      </items>
      <items code="at0004">
        <items id="text">Contact type</items>
        <items id="description">The type of contact where potential transmission occurred.</items>
        <items id="comment">For example: household, health facility,</items>
      </items>
      <items code="at0010">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the contact not captured in other fields.</items>
      </items>
      <items code="at0011">
        <items id="text">Vaccination status</items>
        <items id="description">Assertion about whether the contact has received all recommended and relevant vaccines for the infectious disease or agent.</items>
      </items>
      <items code="at0012">
        <items id="text">Up-to-date</items>
        <items id="description">The individual has been administered all relevant vaccinations.</items>
      </items>
      <items code="at0013">
        <items id="text">Up-to-date for age</items>
        <items id="description">The individual has been administered all vaccinations appropriate for their age.</items>
      </items>
      <items code="at0014">
        <items id="text">Not up-to-date</items>
        <items id="description">The individual has not received relevant vaccinations.</items>
      </items>
      <items code="at0015">
        <items id="text">Identifier</items>
        <items id="description">Administrative identifier or label to anonymise the contact.</items>
      </items>
      <items code="at0016">
        <items id="text">Contacted?</items>
        <items id="description">Has contact been made with the exposed individual?</items>
      </items>
      <items code="at0017">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0018">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0019">
        <items id="text">Unsure</items>
        <items id="description"></items>
      </items>
      <items code="at0020">
        <items id="text">Treated?</items>
        <items id="description">Has the exposed individual been treated?</items>
      </items>
      <items code="at0021">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0022">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0023">
        <items id="text">Unsure</items>
        <items id="description"></items>
      </items>
      <items code="at0024">
        <items id="text">Symptomatic?</items>
        <items id="description">Is the exposed individual reporting any relevant symptoms or signs?</items>
      </items>
      <items code="at0025">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0026">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0027">
        <items id="text">Unsure</items>
        <items id="description"></items>
      </items>
    </term_definitions>
  </ontology>
