  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Three Delays Model (3DM)</items>
        <items id="description">A framework to identify factors that lead to delays or problems in the provision of healthcare and poor outcomes.</items>
        <items id="comment">The Three Delays Model was originally developed&#xD;
in the context of maternal mortality in low income settings but has been used in other contexts.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0005">
        <items id="text">Delay category</items>
        <items id="description">The type of delay.</items>
        <items id="comment">For example: delay related to patient factors; delay related to socio-economic factors; or delay related to health facility factors.</items>
      </items>
      <items code="at0008">
        <items id="text">Description</items>
        <items id="description">Narrative description about the delay category.</items>
      </items>
      <items code="at0009">
        <items id="text">Comment</items>
        <items id="description">Narrative description about the delay category, not captured in other fields.</items>
      </items>
      <items code="at0012">
        <items id="text">Delay details</items>
        <items id="description">Additional details about subcategories of delay or specific delays.</items>
      </items>
      <items code="at0013">
        <items id="text">Delay presence?</items>
        <items id="description">Was the delay recognised as present, relevant to the identified 'Delay category'?</items>
      </items>
      <items code="at0014">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0015">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0018">
        <items id="text">Unknown</items>
        <items id="description"></items>
      </items>
      <items code="at0019">
        <items id="text">Seeking care</items>
        <items id="description">Deciding to seek additional care</items>
      </items>
      <items code="at0020">
        <items id="text">Reaching care</items>
        <items id="description">Reaching an appropriate health facility</items>
      </items>
      <items code="at0021">
        <items id="text">Receiving care</items>
        <items id="description">Receiving appropriate care from the health facility</items>
      </items>
      <items code="at0022">
        <items id="text">Solution responsibility</items>
        <items id="description">The name of the person or organisation assigned responsibility to solve or mitigate the identified delay category.</items>
      </items>
      <items code="at0023">
        <items id="text">Solution timeline</items>
        <items id="description">The due date or proposed timeframe for resolution or mitigation of the delay category.</items>
      </items>
    </term_definitions>
