        <items id="text">Tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Agent</items>
        <items id="description">Identification of the chemical, physical or biological agent to which the subject was exposed.</items>
        <items id="comment">For example: passive smoking or industrial noise.</items>
      </items>
      <items code="at0003">
        <items id="text">Overall Description</items>
        <items id="description">Overall description of the exposure to the identified substance.</items>
      </items>
      <items code="at0004">
        <items id="text">Ongoing Exposure</items>
        <items id="description">Does the subject remain exposed to the substance to a degree that could be regarded as a potential risk to health?</items>
      </items>
      <items code="at0005">
        <items id="text">Category</items>
        <items id="description">Type of exposure.</items>
      </items>
      <items code="at0006">
