  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Management summary</items>
        <items id="description">Summary or persistent details about the outcome of a treatment course or protocol for an identified condition.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0003">
        <items id="text">Management name</items>
        <items id="description">The name of the treatment or course of treatment.</items>
      </items>
      <items code="at0004">
        <items id="text">Date management completed</items>
        <items id="description">The date when the treatment or course of treatment was completed.</items>
      </items>
      <items code="at0005">
        <items id="text">Management status</items>
        <items id="description">Description about the status of the treatment course.</items>
        <items id="comment">For example: completed or not completed.</items>
      </items>
      <items code="at0006">
        <items id="text">Index problem</items>
        <items id="description">The name of the index disease, condition, injury or event.</items>
      </items>
      <items code="at0007">
        <items id="text">Reason for non-completion</items>
        <items id="description">Description of the reason why the course of treatment was not completed.</items>
      </items>
      <items code="at0008">
        <items id="text">Management outcome</items>
        <items id="description">Category of the treatment outcome for the individual.</items>
      </items>
      <items code="at0010">
        <items id="text">Date last seen</items>
        <items id="description">The date when the individual last attended for treatment.</items>
        <items id="comment">May be useful, for example, when the individual is lost to follow-up.</items>
      </items>
      <items code="at0012">
        <items id="text">Issue name</items>
        <items id="description">Name of the issue.</items>
      </items>
      <items code="at0013">
        <items id="text">Issue</items>
        <items id="description">Details about an issue that impacted the treatment outcome.</items>
        <items id="comment">For example: drug resistance</items>
      </items>
      <items code="at0014">
        <items id="text">Date identified</items>
        <items id="description">Date when the issue was identified.</items>
      </items>
      <items code="at0015">
        <items id="text">Comment</items>
        <items id="description">Narrative description about the issue, not captured in other data elements.</items>
      </items>
      <items code="at0016">
        <items id="text">Comment</items>
        <items id="description">Narrative description about the overall treatment, not captured in other data elements.</items>
      </items>
      <items code="at0017">
        <items id="text">Extension</items>
        <items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
        <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
      </items>
      <items code="at0018">
        <items id="text">Last updated</items>
        <items id="description">The date and/or time when the Treatment outcome summary was last updated.</items>
      </items>
      <items code="at0019">
        <items id="text">Additional details</items>
        <items id="description">Additional structured details about the treatment outcome.</items>
        <items id="comment">For example: drug resistance profile; clinical evidence.</items>
      </items>
      <items code="at0021">
        <items id="text">Completed</items>
        <items id="description">The treatment course was completed.</items>
      </items>
      <items code="at0022">
        <items id="text">Not completed</items>
        <items id="description">The treatment course was not completed.</items>
      </items>
      <items code="at0023">
        <items id="text">Treatment successful</items>
        <items id="description">Individual is considered cured; there is no ongoing evidence of the index problem.</items>
      </items>
      <items code="at0024">
        <items id="text">Treatment failed</items>
        <items id="description">Treatment was not successful; there is ongoing evidence of the index problem.</items>
      </items>
      <items code="at0025">
        <items id="text">Indeterminate</items>
        <items id="description">It is not known if the treatment was successful or unsuccessful.</items>
      </items>
      <items code="at0027">
        <items id="text">Reason for outcome</items>
        <items id="description">Reason for a failed treatment outcome.</items>
        <items id="comment">For example: drug resistance.</items>
      </items>
      <items code="at0028">
        <items id="text">Description</items>
        <items id="description">Narrative description about the treatment outcome.</items>
      </items>
      <items code="at0029">
        <items id="text">Indeterminate</items>
        <items id="description">It is not known if the treatment was completed or not.</items>
      </items>
      <items code="at0030">
        <items id="text">Issue details</items>
        <items id="description">Structured details about the issue.</items>
      </items>
      <items code="at0031">
        <items id="text">Reason for management</items>
        <items id="description">The reason for treatment.</items>
        <items id="comment">For example: palliation; relapse; second line treatment after initial failure.</items>
      </items>
    </term_definitions>
  </ontology>
