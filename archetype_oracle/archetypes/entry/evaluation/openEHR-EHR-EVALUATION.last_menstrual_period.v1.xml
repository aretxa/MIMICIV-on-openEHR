  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Last menstrual period</items>
        <items id="description">Identification of the first day of the most recent menstrual cycle for the individual.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Date of onset (LMP)</items>
        <items id="description">Date of onset of menstrual bleeding.</items>
        <items id="comment">Also known as 'LMP'. Useful to persist for calculations including estimated date of delivery in pregnancy, or to distinguish the latest typical menstrual cycle when there is an irregular menstrual pattern.&#xD;
Can be a partial date, for example, only a year.</items>
      </items>
      <items code="at0003">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Last updated</items>
        <items id="description">Date when the Last menstrual period was updated.</items>
      </items>
      <items code="at0006">
        <items id="text">Extension</items>
        <items id="description">Additional information required to capture local content or to align with other reference models/formalisms.</items>
        <items id="comment">For example: local information requirements or additional metadata to align with FHIR.</items>
      </items>
      <items code="at0007">
        <items id="text">Description</items>
        <items id="description">Narrative description about the last menstrual period.</items>
      </items>
      <items code="at0008">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the last menstrual period, not captured in other fields.</items>
      </items>
      <items code="at0009">
        <items id="text">Certainty</items>
        <items id="description">The level of certainty the date was accurate.</items>
        <items id="comment">For example certain or uncertain.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Siste menstruasjonsperiode</items>
        <items id="description">Identifisering av den første dagen i den siste menstruasjonsperioden individet opplevde.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Startdato</items>
        <items id="description">Dato for første blødningsdag.</items>
        <items id="comment">Også kjent som siste menstruasjons første dag. Nyttig å lagre for senere bruk i beregninger som estimert termindato under graviditet, eller for å skille den siste typiske menstruasjonssyklusen fra et uregelmessig menstruasjonsmønster. Kan være en delvis dato, for eksempel bare et årstall.</items>
      </items>
      <items code="at0003">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0004">
        <items id="text">Sist oppdatert</items>
        <items id="description">Dato for når Siste menstruasjonsperiode ble oppdatert.</items>
      </items>
      <items code="at0006">
        <items id="text">Tilleggsinformasjon</items>
        <items id="description">Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.</items>
        <items id="comment">For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.</items>
      </items>
      <items code="at0007">
        <items id="text">Beskrivelse av siste normale menstruasjonsperiode.</items>
        <items id="description">Fritekstbeskrivelse av siste menstruasjonsperiode.</items>
      </items>
      <items code="at0008">
        <items id="text">Kommentar</items>
        <items id="description">Ytterligere fritekst om siste menstruasjonsperiode som ikke er omfattet av andre felt.</items>
      </items>
      <items code="at0009">
        <items id="text">Sikkerhet</items>
        <items id="description">Sikkerhetsgraden for at datoen var korrekt.</items>
        <items id="comment">For eksempel sikker eller usikker.</items>
      </items>
    </term_definitions>
  </ontology>
