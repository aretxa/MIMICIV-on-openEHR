      <items code="at0015">
        <items id="text">Datos de contacto actualizados</items>
        <items id="description">Excluído del cribado por no disponer de datos de contacto actualizados.</items>
      </items>
      <items code="at0016">
        <items id="text">Remisión de la diabetes</items>
        <items id="description">Excluído del cribado por remisión de la diabetes.</items>
      </items>
      <items code="at0017">
        <items id="text">Trastorno físico</items>
        <items id="description">Excluído del cribado por trastorno físico.</items>
      </items>
      <items code="at0018">
        <items id="text">Enfermedad terminal</items>
        <items id="description">Excluído del cribado por enfermedad terminal.</items>
      </items>
      <items code="at0019">
        <items id="text">Bajo el cuidado directo de un oftalmologo</items>
        <items id="description">Excluído del cribado por asignarle bajo el cuidado directo de un oftalmologo.</items>
      </items>
      <items code="at0020">
