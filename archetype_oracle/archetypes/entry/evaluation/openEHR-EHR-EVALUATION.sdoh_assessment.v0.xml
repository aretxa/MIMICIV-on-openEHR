  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Social determinants of health (SDOH) self-assessment</items>
        <items id="description">Self-identified assessment of an individual's sense of safey or security about social determinants of health (SDOH).</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0006">
        <items id="text">Food security</items>
        <items id="description">Does the individual feel that their access to adequate amounts of appropriate food is secure?</items>
      </items>
      <items code="at0007">
        <items id="text">Housing security</items>
        <items id="description">Does the individual feel that their housing situation is adequate and secure?</items>
      </items>
      <items code="at0008">
        <items id="text">Employment security</items>
        <items id="description">Does the individual feel that their working situation is secure or that they have access to appropriate employment if they need it?</items>
      </items>
      <items code="at0009">
        <items id="text">Financial security</items>
        <items id="description">Does the individual feel that their financial situation is safe and secure?</items>
      </items>
      <items code="at0011">
        <items id="text">Personal safety</items>
        <items id="description">Does the individual feel that their situation regarding a sense of personal safety is safe and secure?</items>
        <items id="comment">For example: exposure to violence in the home, racism or discrimination.</items>
      </items>
      <items code="at0012">
        <items id="text">Neighborhood safety</items>
        <items id="description">Does the individual feel that their situation regarding living, working or moving around in their community and local environment is safe and secure?</items>
      </items>
      <items code="at0002">
        <items id="text">Healthcare access</items>
        <items id="description">Does the individual feel that their access to high quality healthcare is adequate and secure?</items>
      </items>
      <items code="at0003">
        <items id="text">Yes</items>
        <items id="description">No problem!</items>
      </items>
      <items code="at0004">
        <items id="text">Sometimes</items>
        <items id="description">Maybe a little wobbly.</items>
      </items>
      <items code="at0005">
        <items id="text">No</items>
        <items id="description">Worrying.</items>
      </items>
      <items code="at0015">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0016">
        <items id="text">Extension</items>
        <items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
        <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
      </items>
      <items code="at0017">
        <items id="text">Last updated</items>
        <items id="description">Date when the SDOH security was last updated.</items>
      </items>
      <items code="at0013">
        <items id="text">Social connection</items>
        <items id="description">Does the individual feel that they have adequate opportunities for social connection?</items>
      </items>
      <items code="at0014">
        <items id="text">Communication</items>
        <items id="description">Does the individual feel that are able to communicate adequately as they need to?</items>
      </items>
      <items code="at0010">
        <items id="text">Education access</items>
        <items id="description">Does the individual feel that their access to high quality education is adequate and secure?</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

