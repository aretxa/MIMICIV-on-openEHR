  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Breast feeding summary</items>
        <items id="description">Summary or persistent information about the all breastfeeding experience of an individual.</items>
      </items>
      <items code="at0001">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">Status</items>
        <items id="description"></items>
      </items>
      <items code="at0013">
        <items id="text">Item tree</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0006">
        <items id="text">Description</items>
        <items id="description">Narrative description about the overall breastfeeding history for an individual.</items>
      </items>
      <items code="at0007">
        <items id="text">Per episode</items>
        <items id="description">Detail about a discrete episode of breastfeeding activity.</items>
      </items>
      <items code="at0010">
        <items id="text">Start date</items>
        <items id="description">The date when the episode of breastfeeding commenced.</items>
      </items>
      <items code="at0011">
        <items id="text">Stop date</items>
        <items id="description">The date when the episode of breastfeeding ceased.</items>
      </items>
      <items code="at0009">
        <items id="text">Episode description</items>
        <items id="description">Narrative description about breastfeeding during the identified episode.</items>
      </items>
      <items code="at0014">
        <items id="text">Last updated</items>
        <items id="description">The date when the breast feeding summary was last updated.</items>
      </items>
      <items code="at0015">
        <items id="text">Extension</items>
        <items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
        <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
      </items>
      <items code="at0003">
        <items id="text">Currently breastfeeding</items>
        <items id="description">The individual is currently breastfeeding.</items>
      </items>
      <items code="at0004">
        <items id="text">Previous breastfeeding</items>
        <items id="description">The individual has breastfed in the past.</items>
      </items>
      <items code="at0005">
        <items id="text">Never breastfed</items>
        <items id="description">The individual has never breastfed.</items>
      </items>
      <items code="at0012">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about all breastfeeding not captured in other fields.</items>
      </items>
      <items code="at0008">
        <items id="text">Episode label</items>
        <items id="description">Identification of an episode of breastfeeding - either as a number in a sequence and/or a named event.</items>
        <items id="comment">For example: '2' as the second episode within a sequence of episodes; or 'James' if describing the breastfeeding after the birth of an infant named James.</items>
      </items>
    </term_definitions>
  </ontology>
