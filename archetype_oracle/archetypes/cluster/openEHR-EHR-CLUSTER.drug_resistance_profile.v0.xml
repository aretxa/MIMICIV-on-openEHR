  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Drug resistance profile</items>
        <items id="description">Information about the reduction in the effectiveness of a medication in treating a condition.</items>
        <items id="comment">For example: an antibiotic to treat an infection or chemotherapy to treat a cancer.</items>
      </items>
      <items code="at0001">
        <items id="text">Index condition</items>
        <items id="description">Identification of the disease or causal microbe.</items>
        <items id="comment">For example: 'Tuberculosis' or 'Mycobacterium tuberculosis'. This data element may be redundant if the disease or causal microbe is identified in the parent archetype.</items>
      </items>
      <items code="at0002">
        <items id="text">Resistance category</items>
        <items id="description">Category of drug resistance observed in relation to the 'Index condition'.</items>
        <items id="comment">For example: 'No resistance', 'Monoresistant', 'Polydrug resistant', 'Multi-drug resistant (MDR), or 'Extensively drug resistant' to Tuberculosis.</items>
      </items>
      <items code="at0003">
        <items id="text">Organism</items>
        <items id="description">Name of the causal organism for an infectious disease.</items>
      </items>
      <items code="at0004">
        <items id="text">Disease</items>
        <items id="description">Name of the disease.</items>
      </items>
      <items code="at0005">
        <items id="text">Specific resistance per drug</items>
        <items id="description">Details about the resistance of the condition or microbe to an identified drug.</items>
      </items>
      <items code="at0006">
        <items id="text">Drug name</items>
        <items id="description">Name of the medication or drug.</items>
      </items>
      <items code="at0007">
        <items id="text">Resistant?</items>
        <items id="description">The organism is resistant to the identified drug?</items>
      </items>
      <items code="at0008">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0010">
        <items id="text">Comment</items>
        <items id="description">Narrative description about the drug resistance profile for the identified condition or microbe, not captured in other fields.</items>
      </items>
      <items code="at0011">
        <items id="text">Description</items>
        <items id="description">Narrative description about the drug resistance profile for the identified disease or causal microbe.</items>
      </items>
    </term_definitions>
  </ontology>
