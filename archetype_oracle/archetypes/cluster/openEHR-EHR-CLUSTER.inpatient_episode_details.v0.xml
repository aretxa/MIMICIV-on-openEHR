  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Inpatient episode details</items>
        <items id="description">Details about an admission episode to an inpatient facility.</items>
      </items>
      <items code="at0001">
        <items id="text">Place of admission</items>
        <items id="description">The physical location where the patient has been admitted.</items>
        <items id="comment">This data element has multiple occurrences to support more than one level of location to be recorded. For example, both the hospital and the ward.</items>
      </items>
      <items code="at0002">
        <items id="text">Structured place of admission</items>
        <items id="description">Structured details about the place of admission.</items>
      </items>
      <items code="at0003">
        <items id="text">Reason for admission</items>
        <items id="description">The reason for admission.</items>
        <items id="comment">For example: admission diagnosis.</items>
      </items>
      <items code="at0004">
        <items id="text">Date of admission</items>
        <items id="description">The date of admission to the inpatient facility.</items>
      </items>
      <items code="at0005">
        <items id="text">Date of separation</items>
        <items id="description">The date of when the individual left to the inpatient facility.</items>
        <items id="comment">For example: discharged home or to an aged care facility.</items>
      </items>
      <items code="at0006">
        <items id="text">Separation outcome</items>
        <items id="description">The state of the individual as they left the facility.</items>
        <items id="comment">For example: alive and well; dead.</items>
      </items>
    </term_definitions>
  </ontology>
