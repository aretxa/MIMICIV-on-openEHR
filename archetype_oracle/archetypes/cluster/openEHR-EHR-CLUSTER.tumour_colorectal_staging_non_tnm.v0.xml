  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Tumour - Colorectal staging (non-TNM)</items>
        <items id="description">Non-TNM staging scores for colorectal cancer.</items>
      </items>
      <items code="at0001">
        <items id="text">Dukes Score</items>
        <items id="description">Dukes classification for colorectal cancer (Dukes and Bussey modification).</items>
      </items>
      <items code="at0002">
        <items id="text">ACPS Score</items>
        <items id="description">Australian clinicopathological staging (ACPS) system.</items>
      </items>
      <items code="at0007">
        <items id="text">Dukes A</items>
        <items id="description">Tumour limited to the wall of the bowel, lymph nodes negative.</items>
      </items>
      <items code="at0008">
        <items id="text">Dukes B</items>
        <items id="description">Tumour spread beyond muscularis propria, lymph nodes negative.</items>
      </items>
      <items code="at0009">
        <items id="text">Dukes C1</items>
        <items id="description">Lymph nodes positive but highest node spared.</items>
      </items>
      <items code="at0010">
        <items id="text">Dukes C2</items>
        <items id="description">Highest lymph node involved.</items>
      </items>
      <items code="at0011">
        <items id="text">Stage A0</items>
        <items id="description">Mucosa involved.</items>
      </items>
      <items code="at0012">
        <items id="text">Stage A</items>
        <items id="description">Submucosa involved.</items>
      </items>
      <items code="at0013">
        <items id="text">Stage B</items>
        <items id="description">Muscularis propria involved.</items>
      </items>
      <items code="at0014">
        <items id="text">Stage C</items>
        <items id="description">Local nodes involved.</items>
      </items>
      <items code="at0015">
        <items id="text">Stage D</items>
        <items id="description">Tumour transected (histological).</items>
      </items>
      <items code="at0016">
        <items id="text">ACPS Concord variant</items>
        <items id="description">Concord substage variant of Australian clinicopathological staging (ACPS) system.</items>
      </items>
      <items code="at0017">
        <items id="text">Stage A1</items>
        <items id="description">Mucosa involved.</items>
      </items>
      <items code="at0018">
        <items id="text">Stage A2</items>
        <items id="description">Submucosa involved.</items>
      </items>
      <items code="at0019">
        <items id="text">Stage A3</items>
        <items id="description">Muscularis propria involved.</items>
      </items>
      <items code="at0020">
        <items id="text">Stage B1</items>
        <items id="description">Involvement beyond muscularis propria.</items>
      </items>
      <items code="at0021">
        <items id="text">Stage B2</items>
        <items id="description">Involvement of free serosal surface.</items>
      </items>
      <items code="at0022">
        <items id="text">Stage C1</items>
        <items id="description">Local nodes involved.</items>
      </items>
      <items code="at0023">
        <items id="text">Stage C2</items>
        <items id="description">Apical nodes involved.</items>
      </items>
      <items code="at0024">
        <items id="text">Stage D1</items>
        <items id="description">Tumour transected (histological).</items>
      </items>
      <items code="at0025">
        <items id="text">Stage D2</items>
        <items id="description">Distant metastases (clinical or histological).</items>
      </items>
    </term_definitions>
    <term_definitions language="ar-sy">
      <items code="at0000">
        <items id="text">الورم - تحديد مرحلة سرطان القولون و المستقيم (بطريقة مختلفة عن تصنيف (و ل م))</items>
        <items id="description">تحديد أحراز مرحلة سرطان القولون و المستقيم بطريقة مختلفة عن تصنيف (و ل م)</items>
      </items>
      <items code="at0001">
        <items id="text">حرز ديوكس</items>
        <items id="description">تصنيف ديوكس لسرطان القولون و المستقيم (تعديل ديوكس و باسي)</items>
      </items>
      <items code="at0002">
        <items id="text">حرز النظام الأسترالي لتحديد المرحلة السريرية و الباثولوجية</items>
        <items id="description">النظام الأسترالي لتحديد المرحلة السريرية الباثولوجية </items>
      </items>
      <items code="at0007">
        <items id="text">ديوكس أ</items>
        <items id="description">الورم محدود في جدار الأمعاء, و لم يصل إلى العقد الليمفاوية</items>
      </items>
      <items code="at0008">
        <items id="text">ديوكس ب</items>
        <items id="description">انتشر الورم فيما يتجاوز النسيج العضلي المخصوص, و لم يصل إلى العقد الليمفاوية</items>
      </items>
      <items code="at0009">
        <items id="text">ديوكس ج 1</items>
        <items id="description">العقد الليمفاوية بها ورم, و لكن العقد المرتفعة في الجسم خالية من الورم</items>
      </items>
      <items code="at0010">
        <items id="text">ديوكس ج 2</items>
        <items id="description">حدث اكتناف للعقد الليمفاوية العلوية</items>
      </items>
      <items code="at0011">
        <items id="text">المرحلة أ 0</items>
        <items id="description">تم اكتناف النسيج المخاطي</items>
      </items>
      <items code="at0012">
        <items id="text">المرحلة أ</items>
        <items id="description">تم اكتناف النسيج تحت المخاطي</items>
      </items>
      <items code="at0013">
        <items id="text">المرحلة ب</items>
        <items id="description">تم اكتناف النسيج العضلي المخصوص</items>
      </items>
      <items code="at0014">
        <items id="text">المرحلة ج</items>
        <items id="description">تم اكتناف العقد الليمفاوية الموضعية</items>
      </items>
      <items code="at0015">
        <items id="text">المرحلة د</items>
        <items id="description">الورم مقطوع - هيستولوجي</items>
      </items>
      <items code="at0016">
        <items id="text">متغير التوافق المتعلق بالنظام الأسترالي لتحديد المرحلة السريرية و الباثولوجية</items>
        <items id="description">متغير التوافق المتعلق بالنظام الأسترالي لتحديد(تحت المرحلة) السريرية و الباثولوجية&#xD;
</items>
      </items>
      <items code="at0017">
        <items id="text">المرحلة 1 أ</items>
        <items id="description">تم اكتناف الغشاء المخاطي</items>
      </items>
      <items code="at0018">
        <items id="text">المرحلة 2 أ</items>
        <items id="description">تم اكتناف الغشاء تحت المخاطي</items>
      </items>
      <items code="at0019">
        <items id="text">المرحلة 3 أ</items>
        <items id="description">تم اكتناف النسيج العضلي المخصوص</items>
      </items>
      <items code="at0020">
        <items id="text">المرحلة ب 1</items>
        <items id="description">الاكتناف يتجاوز النسيج العضلي المخصوص</items>
      </items>
      <items code="at0021">
        <items id="text">المرحلة ب 2</items>
        <items id="description">اكتناف السطح المَصْلي الحر </items>
      </items>
      <items code="at0022">
        <items id="text">المرحلة ج 1</items>
        <items id="description">تم اكتناف العقد الليمفاوية</items>
      </items>
      <items code="at0023">
        <items id="text">المرحلة ج 2</items>
        <items id="description">تم اكتناف العقد القِمِّية</items>
      </items>
      <items code="at0024">
        <items id="text">المرحلة د 1</items>
        <items id="description">تم قطع الورم (هيستولوجي)</items>
      </items>
      <items code="at0025">
        <items id="text">المرحلة د 2</items>
        <items id="description">توجد نقائل بعيدة (سريرية أو هيستولوجية)</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

