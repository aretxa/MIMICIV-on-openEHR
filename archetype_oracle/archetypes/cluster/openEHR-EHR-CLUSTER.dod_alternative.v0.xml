      <items code="at0001">
        <items id="text">Possible date/time of death</items>
        <items id="description">Proposed alternative date of death.</items>
        <items id="comment">For example: Partial dates, estimated date/time of death, approximate date/time of death.</items>
      </items>
      <items code="at0002">
        <items id="text">Source</items>
        <items id="description">Source of information about the possible date/time of death.</items>
      </items>
      <items code="at0003">
        <items id="text">Comment</items>
        <items id="description">Additional information about the possible date/time of death, not captured in other data elements.</items>
      </items>
      <items code="at0004">
        <items id="text">Certainty</items>
        <items id="description">Degree of certainty of timing.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Dødsfalltidspunkt alternativ</items>
        <items id="description">Dødsfalltidspunktdetaljer.</items>
      </items>
      <items code="at0001">
        <items id="text">Mulig dødsfalltidspunkt</items>
        <items id="description">Foreslått alternativ dødstidspunkt.</items>
        <items id="comment">For eksempel: Deldato, utregnet dødstidspunkt, omtrentlig dødstidspunkt.</items>
      </items>
      <items code="at0002">
        <items id="text">Kilde</items>
        <items id="description">Informasjonskilde om den mulige dødsfalltidspunkt.</items>
      </items>
      <items code="at0003">
        <items id="text">Kommentar</items>
        <items id="description">Ytterligere informasjon om mulige dødsfalltidspunkt som ikke er fanget i andre felt.</items>
      </items>
      <items code="at0004">
        <items id="text">Visshet</items>
        <items id="description">Nivå av visshet rundt tidsangivelse.</items>
      </items>
    </term_definitions>
