  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Auscultation of bowel sounds</items>
        <items id="description">Findings observed during the auscultation of bowel sounds.</items>
      </items>
      <items code="at0001">
        <items id="text">Body site</items>
        <items id="description">Identification of the area of the body under examination.</items>
        <items id="comment">For example: right lower quadrant or left lower chest. If the body site has been fully identified in the parent archetype in which this archetype is nested, this data element becomes redundant.</items>
      </items>
      <items code="at0003">
        <items id="text">Clinical description</items>
        <items id="description">Narrative description of the auscultation findings.</items>
      </items>
      <items code="at0004">
        <items id="text">Presence</items>
        <items id="description">The presence or absence of any bowel sounds heard on auscultation.</items>
      </items>
      <items code="at0012">
        <items id="text">Clinical interpretation</items>
        <items id="description">Single word, phrase or brief description that represents the clinical meaning and significance of the auscultation  findings at the body site.</items>
        <items id="comment">For example: 'No abnormality detected' or 'Decreased intestinal motility'. Coding of the 'Clinical interpretation' with a terminology is preferred, where possible.</items>
      </items>
      <items code="at0013">
        <items id="text">Examination not done</items>
        <items id="description">Details to explicitly record that this examination was not performed.</items>
      </items>
      <items code="at0014">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the auscultation findings at the body site, not captured in other fields.</items>
      </items>
      <items code="at0015">
        <items id="text">Structured body site</items>
        <items id="description">A structured description of the area of the body under examination.</items>
        <items id="comment">If the body site has been fully identified in the parent archetype in which this archetype is nested or in the 'Body site' data element, this SLOT becomes redundant.</items>
      </items>
      <items code="at0017">
        <items id="text">Intensity</items>
        <items id="description">The relative volume or loudness of all bowel sounds compared to what is expected.</items>
      </items>
      <items code="at0018">
        <items id="text">Increased</items>
        <items id="description">The bowel sound is louder than expected over the area of abdomen being auscultated.</items>
      </items>
      <items code="at0019">
        <items id="text">Normal</items>
        <items id="description">The bowel sound is heard at the expected intensity over the area of abdomen being auscultated.</items>
      </items>
      <items code="at0020">
        <items id="text">Decreased</items>
        <items id="description">The bowel sound is less intense than expected over the area of abdomen being auscultated.</items>
      </items>
      <items code="at0021">
        <items id="text">Absent</items>
        <items id="description">The bowel sound is not heard over the area of abdomen being auscultated.</items>
      </items>
      <items code="at0022">
        <items id="text">Present</items>
        <items id="description"></items>
      </items>
      <items code="at0023">
        <items id="text">Absent</items>
        <items id="description"></items>
      </items>
      <items code="at0025">
        <items id="text">Type</items>
        <items id="description">The type or character of bowel sounds heard on auscultation.</items>
        <items id="comment">For example: Tinkling.</items>
      </items>
      <items code="at0026">
        <items id="text">Frequency</items>
        <items id="description">The assessment of the relative frequency of bowel sounds heard on auscultation.</items>
      </items>
      <items code="at0027">
        <items id="text">Hypoactive</items>
