  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Tos Classification</items>
        <items id="description">A classification of the degree of tympanic membrane pars flaccida retraction, related to extent and severity and as described by Tos and Poulsen.</items>
      </items>
      <items code="at0001">
        <items id="text">Tos Classification</items>
        <items id="description">Grading of the degree of tympanic membrane pars tensa retraction / atelectasis based on the Tos &amp; Poulson Classification.</items>
      </items>
      <items code="at0002">
        <items id="text">Grade 1</items>
        <items id="description">Pars flaccida is dimpled and is more retracted than normal. It is not adherent to the malleus (airspace visible).</items>
      </items>
      <items code="at0003">
        <items id="text">Grade 2</items>
        <items id="description">Retraction onto neck of malleus - no airspace visible behind membrane.</items>
      </items>
      <items code="at0004">
        <items id="text">Grade 3</items>
        <items id="description">Retraction extends beyond osseous malleus full extent seen.</items>
      </items>
      <items code="at0005">
        <items id="text">Grade 4</items>
        <items id="description">Erosion of outer attic wall. Part of the retraction pocket may be hidden.</items>
      </items>
      <items code="at0006">
        <items id="text">Grade 5</items>
        <items id="description">Bottom of the retraction pocket can be seen only by using an endoscope.</items>
      </items>
    </term_definitions>
  </ontology>
