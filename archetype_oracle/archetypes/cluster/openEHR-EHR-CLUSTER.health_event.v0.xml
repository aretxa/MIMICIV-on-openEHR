  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Health event</items>
        <items id="description">Details about a specific health-related event.</items>
        <items id="comment">For example: a poisoning, overdose or trauma.</items>
      </items>
      <items code="at0001">
        <items id="text">Event name</items>
        <items id="description">Identification of the event that occurred.</items>
      </items>
      <items code="at0007">
        <items id="text">Circumstances</items>
        <items id="description">Narrative description the context and circumstances surrounding the event.</items>
      </items>
      <items code="at0002">
        <items id="text">Description</items>
        <items id="description">A narrative description of the event.</items>
      </items>
      <items code="at0005">
        <items id="text">Time elapsed</items>
        <items id="description">The time between the event and the time of clinical assessment.</items>
      </items>
      <items code="at0008">
        <items id="text">Contributing factor</items>
        <items id="description">Identification of factors contributing to the occurrence of the event.</items>
      </items>
      <items code="at0003">
        <items id="text">Preceding symptoms and events</items>
        <items id="description">Symptoms and events which preceded the index event.</items>
      </items>
      <items code="at0004">
        <items id="text">Associated symptoms and events</items>
        <items id="description">Symptoms and events which occurred at the time of the index event.</items>
      </items>
      <items code="at0009">
        <items id="text">Additional details</items>
        <items id="description">Structured details about specific aspects of the event.</items>
      </items>
      <items code="at0006">
        <items id="text">Witness</items>
        <items id="description">Identification of an individual who witnessed the event.</items>
      </items>
      <items code="at0010">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the health event, not captured in other fields.</items>
      </items>
    </term_definitions>
