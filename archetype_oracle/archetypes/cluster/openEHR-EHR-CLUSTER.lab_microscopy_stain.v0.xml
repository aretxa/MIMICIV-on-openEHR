  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Microscopy stain findings</items>
        <items id="description">Findings observed during microscopic examination of tissue treated with specialised stains, intended to enhance the visualisation of specimen, including microorganisms, tissue structures and cellular details.</items>
      </items>
      <items code="at0004">
        <items id="text">Staining method</items>
        <items id="description">Name of the technique used to stain the specimen.</items>
        <items id="comment">For example: ZiehlâNeelsen; Gram stain</items>
      </items>
      <items code="at0005">
        <items id="text">Gram stain</items>
        <items id="description"></items>
      </items>
      <items code="at0006">
        <items id="text">India ink stain</items>
        <items id="description"></items>
      </items>
      <items code="at0007">
        <items id="text">Ziehl-Neelsen stain</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">Presence</items>
        <items id="description">Statement about detection of the staining target in the specimen.</items>
      </items>
      <items code="at0010">
        <items id="text">Detected</items>
        <items id="description"></items>
      </items>
      <items code="at0011">
        <items id="text">Not detected</items>
        <items id="description"></items>
      </items>
      <items code="at0012">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the overall microscopy findings using the identified stain, not captured in other fields.</items>
      </items>
      <items code="at0014">
        <items id="text">Specimen</items>
        <items id="description"></items>
      </items>
      <items code="at0015">
        <items id="text">Staining agent</items>
        <items id="description">A chemical dye or compound that interacts with biological specimens to produce a colour change, improving their visibility and differentiation under a microscope.</items>
        <items id="comment">For example: crystal violet, carbol fuchsin.</items>
      </items>
      <items code="at0016">
        <items id="text">Per target finding</items>
        <items id="description">Details about a specific microorganism, tissue structure, or cell structure observed during the microscopic examination of a stained specimen.</items>
      </items>
      <items code="at0017">
        <items id="text">Staining target</items>
        <items id="description">Identification of the target organism, cell or structure.</items>
        <items id="comment">For example: gram-positive cocci, fungi, acid-fast bacilli, cell nuclei or pathological inclusions.&#xD;
Coding with an external terminology is preferred, if available.</items>
      </items>
      <items code="at0018">
        <items id="text">Qualitative result</items>
