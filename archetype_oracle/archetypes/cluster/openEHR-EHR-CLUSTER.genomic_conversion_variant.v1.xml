  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Genomic conversion variant</items>
        <items id="description">A human genetic sequence change where, compared to a genomic reference sequence, a range of nucleotides are replaced by a sequence from elsewhere in the genome.</items>
      </items>
      <items code="at0003">
        <items id="text">Start converted position</items>
        <items id="description">The position of the first nucleotide of the converted range.</items>
      </items>
      <items code="at0004">
        <items id="text">End converted position</items>
        <items id="description">The position of the last nucleotide of the converted range.</items>
      </items>
      <items code="at0009">
        <items id="text">Replacing sequence start position</items>
        <items id="description">The position of the first nucleotide of the replacing sequence.</items>
      </items>
      <items code="at0010">
        <items id="text">Replacing sequence end position</items>
        <items id="description">The position of the last nucleotide of the replacing sequence.</items>
      </items>
      <items code="at0011">
        <items id="text">Converted reference sequence</items>
        <items id="description">The sequence file used as a reference to describe the converted region.</items>
      </items>
      <items code="at0012">
        <items id="text">Replacing reference sequence</items>
        <items id="description">The sequence file used as a reference to describe the replacing region.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Genetisk variant - genkonversjon</items>
        <items id="description">En endring av human gensekvensen hvor en rekke nukleotider blir erstattes av en sekvens fra andre steder i genomet, sammenliknet med referansesekvensen.</items>
      </items>
      <items code="at0003">
        <items id="text">Startposisjon til konvertert sekvens</items>
        <items id="description">Posisjonen til det første nukleotidet i det konverterte/erstattede området.</items>
      </items>
      <items code="at0004">
        <items id="text">Sluttposisjonen til konvertert sekvens</items>
        <items id="description">Posisjonen til det siste nukleotidet i det konverterte/erstattede området.</items>
      </items>
      <items code="at0009">
        <items id="text">Startposisjon til erstatningssekvensen</items>
        <items id="description">Posisjonen til det første nukleotidet i sekvensen som har erstattet den opprinnelig sekvensen.</items>
      </items>
      <items code="at0010">
        <items id="text">Sluttposisjon til erstatningssekvensen</items>
        <items id="description">Posisjonen til det siste nukleotidet i sekvensen som har erstattet den opprinnelig sekvensen.</items>
      </items>
      <items code="at0011">
        <items id="text">Konvertert referansesekvens</items>
        <items id="description">Sekvensfilen som har blitt brukt som en referanse for å beskrive det området som har blitt konvertert (erstattet).</items>
      </items>
      <items code="at0012">
        <items id="text">Erstatnings referansesekvens</items>
        <items id="description">Sekvensfilen som brukes som referanse for å beskrive det området som har erstattet den opprinnelige sekvensen.</items>
      </items>
    </term_definitions>
    <term_definitions language="de">
      <items code="at0000">
        <items id="text">Genomic conversion variant</items>
        <items id="description">Eine menschliche genetische Sequenzänderung, bei der im Vergleich zu einer genomischen Referenzsequenz eine Reihe von Nukleotiden durch eine Sequenz von einer anderen Stelle im Genom ersetzt wird.</items>
      </items>
      <items code="at0003">
        <items id="text">Start converted position</items>
        <items id="description">The position of the first nucleotide of the converted range.</items>
      </items>
      <items code="at0004">
        <items id="text">End converted position</items>
        <items id="description">The position of the last nucleotide of the converted range.</items>
      </items>
      <items code="at0009">
        <items id="text">Replacing sequence start position</items>
        <items id="description">The position of the first nucleotide of the replacing sequence.</items>
      </items>
      <items code="at0010">
        <items id="text">Replacing sequence end position</items>
        <items id="description">The position of the last nucleotide of the replacing sequence.</items>
      </items>
      <items code="at0011">
        <items id="text">Converted reference sequence</items>
        <items id="description">The sequence file used as a reference to describe the converted region.</items>
      </items>
      <items code="at0012">
        <items id="text">Replacing reference sequence</items>
        <items id="description">The sequence file used as a reference to describe the replacing region.</items>
      </items>
    </term_definitions>
  </ontology>
