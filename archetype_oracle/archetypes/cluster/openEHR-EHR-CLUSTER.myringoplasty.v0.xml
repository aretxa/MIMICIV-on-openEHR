  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Myringoplasty Procedure</items>
        <items id="description">Surgical procedure in which a performation of the pars tensa of the tympanic membrane is closed.</items>
      </items>
      <items code="at0001">
        <items id="text">Revision?</items>
        <items id="description">Is this operation a revision of a previous myringoplasty?</items>
        <items id="comment">Record as 'True' if operation is a revision of a previous myringoplasty.</items>
      </items>
      <items code="at0002">
        <items id="text">Approach</items>
        <items id="description">The operative approach to the tympanic membrane.</items>
      </items>
      <items code="at0003">
        <items id="text">Transcanal</items>
        <items id="description">Approach through the ear canal, without incision.</items>
      </items>
      <items code="at0004">
        <items id="text">Endaural</items>
        <items id="description">Approach through the ear canal.</items>
      </items>
      <items code="at0005">
        <items id="text">Postaural</items>
        <items id="description">Approach via an incision behind the ear.</items>
      </items>
      <items code="at0006">
        <items id="text">Graft Material</items>
        <items id="description">Material used to repair the perforation.</items>
      </items>
      <items code="at0007">
        <items id="text">Temporal Fascia</items>
        <items id="description">Fascial tissue covering the temporalis muscle was used.</items>
      </items>
      <items code="at0008">
        <items id="text">Cartilage</items>
        <items id="description">Cartilage was used.</items>
      </items>
      <items code="at0009">
        <items id="text">Cartilage/Perichondrium</items>
        <items id="description">Cartilage and perichondrial tissue was used.</items>
      </items>
      <items code="at0010">
        <items id="text">Fat</items>
        <items id="description">Fat tissue was used.</items>
      </items>
      <items code="at0011">
        <items id="text">Other</items>
        <items id="description">Other tissue was used.</items>
      </items>
      <items code="at0012">
        <items id="text">Technique</items>
        <items id="description">The technique used to perform the repair of the perforation.</items>
      </items>
      <items code="at0013">
        <items id="text">Underlay</items>
        <items id="description">An underlay technique was used.</items>
      </items>
      <items code="at0014">
        <items id="text">Inlay</items>
        <items id="description">An inlay technique was used.</items>
      </items>
      <items code="at0015">
        <items id="text">Onlay</items>
        <items id="description">An onlay technique was used.</items>
      </items>
      <items code="at0016">
        <items id="text">Butterfly</items>
        <items id="description">A butterfly technique was used.</items>
      </items>
      <items code="at0017">
        <items id="text">Patch</items>
        <items id="description">A patch technique was used.</items>
      </items>
    </term_definitions>
  </ontology>
