  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Examination of a cleavage-stage embryo</items>
        <items id="description">Morphological findings obtained by microscopy of the human cleavage-stage embryo.</items>
      </items>
      <items code="at0028">
        <items id="text">Number of cells</items>
        <items id="description">Number of cells in a cleavage-stage embryo.</items>
      </items>
      <items code="at0030">
        <items id="text">Fragmentation</items>
        <items id="description">Cytoplasmic fragmentation in a cleavage-stage embryo.</items>
        <items id="comment">The proportion data type can be used to record a more precise assessment.</items>
      </items>
      <items code="at0031">
        <items id="text">None</items>
        <items id="description">Absence of cytoplasmic fragments.</items>
      </items>
      <items code="at0032">
        <items id="text">Mild fragmentation</items>
        <items id="description">Cytoplasmic fragments cover &lt; 10% of the total cytoplasmic volume.</items>
      </items>
      <items code="at0033">
        <items id="text">Moderate fragmentation</items>
        <items id="description">Cytoplasmic fragments cover 10 - 25% of the total cytoplasmic volume.</items>
      </items>
      <items code="at0034">
        <items id="text">Severe fragmentation</items>
        <items id="description">Cytoplasmic fragments cover &gt; 25% of the total cytoplasmic volume.</items>
      </items>
      <items code="at0035">
        <items id="text">Blastomere size</items>
        <items id="description">Relative blastomere size.</items>
      </items>
      <items code="at0036">
        <items id="text">Equal, stage specific</items>
        <items id="description">Equally sized blastomeres, stage specific cell size.</items>
      </items>
      <items code="at0037">
        <items id="text">Unequal, stage specific</items>
        <items id="description">Unequally sized blastomeres, stage specific cell size.</items>
      </items>
      <items code="at0038">
        <items id="text">Nucleation</items>
        <items id="description">Presence or absence of nuclei in the blastomeres of the cleavage-stage embryo.</items>
      </items>
      <items code="at0039">
        <items id="text">No visible nuclei</items>
        <items id="description">Nuclei are not visible.</items>
      </items>
      <items code="at0040">
        <items id="text">Mononucleation</items>
        <items id="description">Only a single visible nucleus in all blastomeres.</items>
      </items>
      <items code="at0041">
        <items id="text">Binucleation</items>
        <items id="description">Two visible nuclei in one blastomere.</items>
      </items>
      <items code="at0042">
        <items id="text">Cytoplasmic morphology</items>
        <items id="description">Morphology of the cytoplasm of cells in cleavage-stage embryo.</items>
        <items id="comment">May contain description of anomalies, such as granularity, pitting, and presence of vacuoles.</items>
      </items>
      <items code="at0043">
        <items id="text">Spatial distribution of cells</items>
        <items id="description">Spatial distribution of cells in a cleavage-stage embryo.</items>
        <items id="comment">A candidate value cannot be identified as the time of publication. If consensus can be achieved in the future, it can be added to the archetype. In the meantime, a use case-appropriate value set can be added in the template.</items>
      </items>
      <items code="at0044">
        <items id="text">Compaction</items>
        <items id="description">Degree of compaction in a cleavage-stage embryo.</items>
      </items>
      <items code="at0045">
        <items id="text">None</items>
        <items id="description">Absence of compaction.</items>
      </items>
      <items code="at0046">
        <items id="text">Minimal</items>
        <items id="description">Less than 50% of the total embryonic volume is compacted.</items>
      </items>
      <items code="at0047">
        <items id="text">Moderate</items>
        <items id="description">Over 50% of the total embryonic volume is compacted.</items>
      </items>
      <items code="at0048">
        <items id="text">Complete</items>
        <items id="description">The complete volume of the embryo is compacted.</items>
      </items>
      <items code="at0049">
        <items id="text">Other morphological features</items>
        <items id="description">Additional morphological features not captured by specific parameters.</items>
        <items id="comment">The occurrences for this data element is set to 0..* to allow for more than one morphological attribute to be added as a coded value.</items>
      </items>
      <items code="at0051">
        <items id="text">Multinucleation</items>
        <items id="description">Multiple visible nuclei in one blastomere.</items>
      </items>
      <items code="at0052">
        <items id="text">Broad multinucleation</items>
        <items id="description">Two or more visible nuclei in two or more blastomeres.</items>
      </items>
      <items code="at0053">
        <items id="text">Equal, non-stage specific</items>
        <items id="description">Equally sized blastomeres, non-stage specific cell size.</items>
      </items>
      <items code="at0054">
        <items id="text">Unequal, non-stage specific</items>
        <items id="description">Unequally sized blastomeres, non-stage specific cell size.</items>
      </items>
      <items code="at0055">
        <items id="text">Morphology grade</items>
        <items id="description">Overall grading or score based on recorded morphological features of the cleavage-stage embryo.</items>
        <items id="comment">Use of an established morphology assessment classification is preferred.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Undersøkelse av et humant embryo i tidlige delingsstadier</items>
        <items id="description">Morfologiske funn ved mikroskopisk undersøkelse av et humant embryo i tidlige delingsstadier.</items>
      </items>
      <items code="at0028">
        <items id="text">Antall celler</items>
        <items id="description">Antall celler i embryoet.</items>
      </items>
      <items code="at0030">
        <items id="text">Fragmenteringsgrad</items>
        <items id="description">Grad av cytoplasmisk fragmentering i et embryo i delingsstadie.</items>
        <items id="comment">For en mer presis vurdering, kan datatypen Proportion brukes.</items>
      </items>
      <items code="at0031">
        <items id="text">Ingen</items>
        <items id="description">Ingen cytoplasmatiske fragmenter.</items>
      </items>
      <items code="at0032">
        <items id="text">Mild fragmentering</items>
        <items id="description">Cytoplasmatiske fragmenter dekker mindre enn 10% av totalt cytoplasmatisk volum.</items>
      </items>
      <items code="at0033">
        <items id="text">Moderat fragmentering</items>
        <items id="description">Cytoplasmatiske fragmenter dekker 10 - 25% av totalt cytoplasmatisk volum.</items>
      </items>
      <items code="at0034">
        <items id="text">Alvorlig fragmentering</items>
        <items id="description">Cytoplasmatiske fragmenter dekker mer enn 25% av totalt cytoplasmatisk volum.</items>
      </items>
      <items code="at0035">
        <items id="text">Cellestørrelse</items>
        <items id="description">Relativ størrelse på cellene i embryoet.</items>
      </items>
      <items code="at0036">
        <items id="text">Lik, stadium-spesifikk</items>
        <items id="description">Celler av lik, stadium-spesifikk strørrelse.</items>
      </items>
      <items code="at0037">
        <items id="text">Ulik, stadium-spesifikk</items>
        <items id="description">Celler av ulik, stadium-spesifikk strørrelse.</items>
      </items>
      <items code="at0038">
        <items id="text">Kjerner</items>
        <items id="description">Tilstedeværelse eller fravær av kjerner i embryoets celler i tidlig delingsstadie.</items>
      </items>
      <items code="at0039">
        <items id="text">Ingen synlige kjerner</items>
        <items id="description">Kjernene er ikke synlige.</items>
      </items>
      <items code="at0040">
        <items id="text">Enkeltkjernede celler</items>
        <items id="description">Bare én kjerne sees i alle cellene i embryoet.</items>
      </items>
      <items code="at0041">
        <items id="text">Tokjernet celle</items>
        <items id="description">To synlige kjerner i en celle.</items>
      </items>
      <items code="at0042">
        <items id="text">Morfologi av cytoplasma</items>
        <items id="description">Morfologi av cytoplasmaet i cellene til embryoet i tidlig delingsstadium.</items>
        <items id="comment">Kan brukes for å beskrive avvikende cytoplasmamorfologi, slik som granulering, dannelse av groper og vakuoler.</items>
      </items>
      <items code="at0043">
        <items id="text">Organisering av celler</items>
        <items id="description">Geometrisk organisering av cellene i embryoet.</items>
        <items id="comment">Ved publikasjon av arketypen finnes det ingen omforent verdisett for dette elementet. Hvis det oppnås konsensus om dette i fremtiden, kan det legges til i arketypen. I mellomtiden, bruk et verdisett som passer til bruksområdet, og legg det til i templat.</items>
      </items>
      <items code="at0044">
        <items id="text">Kompaktering</items>
        <items id="description">Grad av kompaktering i embryo i delingsstadiene.</items>
      </items>
      <items code="at0045">
        <items id="text">Ingen</items>
        <items id="description">Ingen kompaktering.</items>
      </items>
      <items code="at0046">
        <items id="text">Minimal</items>
        <items id="description">Mindre enn 50% av embryoets totalt volum er kompaktert.</items>
      </items>
      <items code="at0047">
        <items id="text">Moderat</items>
        <items id="description">Over 50% av embryoets totalt volum er kompaktert.</items>
      </items>
      <items code="at0048">
        <items id="text">Komplett</items>
        <items id="description">Alle embryoets celler er kompaktert.</items>
      </items>
      <items code="at0049">
        <items id="text">Andre morfologiske observasjoner</items>
        <items id="description">Ytterligere morfologiske observasjoner av embryoet som ikke dekkes av de andre spesifiserte elementene.</items>
        <items id="comment">Dette dataelementet er mulig å repetere for å legge til rette for flere verdier skal kunne velges fra en liste av kodede verdier.</items>
      </items>
      <items code="at0051">
        <items id="text">Flerkjernet celle</items>
        <items id="description">Flere synlige kjerner i en celle.</items>
      </items>
      <items code="at0052">
        <items id="text">Flerkjernede celler.</items>
        <items id="description">To eller flere synlige kjerner i to eller flere celler.</items>
      </items>
      <items code="at0053">
        <items id="text">Lik, ikke stadium-spesifikk</items>
        <items id="description">Celler av lik, ikke stadium-spesifikk strørrelse.</items>
      </items>
      <items code="at0054">
        <items id="text">Ulik, ikke stadium-spesifikk</items>
        <items id="description">Celler av ulik, ikke stadium-spesifikk strørrelse.</items>
      </items>
      <items code="at0055">
        <items id="text">Morfologisk gradering</items>
        <items id="description">Overordnet gradering eller skår basert på embryoets registrerte, morfologiske egenskaper.</items>
        <items id="comment">Bruk av en etablert skala for vurdering av embryoets morfologi anbefales.</items>
      </items>
    </term_definitions>
  </ontology>
