      <items code="at0000">
        <items id="text">Mood Level (EMPOWER)</items>
        <items id="description">Mood level provides a description of the mood state.</items>
      </items>
      <items code="at0001">
        <items id="text">Mood</items>
        <items id="description">Indicates the current mood state of the patient.</items>
      </items>
      <items code="at0003">
        <items id="text">Cheerful</items>
        <items id="description">Highly pleasant emotion characterized by outward manifestations of gratification; joy.</items>
      </items>
      <items code="at0004">
        <items id="text">Euthymic</items>
        <items id="description">Euthymic mood.</items>
      </items>
      <items code="at0005">
        <items id="text">Indifferent</items>
        <items id="description">Lack of emotion or emotional expression; a disorder of motivation that persists over time.</items>
      </items>
      <items code="at0006">
        <items id="text">Sad</items>
        <items id="description">Feeling sad.</items>
      </items>
      <items code="at0007">
        <items id="text">Depressed</items>
        <items id="description">Feelings of grief or unhappiness.</items>
      </items>
    </term_definitions>
