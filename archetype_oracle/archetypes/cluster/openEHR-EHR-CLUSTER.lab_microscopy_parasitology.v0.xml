  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Microbiology parasitology findings</items>
        <items id="description">A laboratory test to directly detect, identify and quantify parasites or their developmental forms in a specimen, using microscopy.</items>
      </items>
      <items code="at0001">
        <items id="text">Clinical description</items>
        <items id="description">Narrative description about all parasitology-related findings on microscopy.</items>
      </items>
      <items code="at0002">
        <items id="text">Specimen</items>
        <items id="description"></items>
      </items>
      <items code="at0003">
        <items id="text">Per parasite</items>
        <items id="description">Per targeted parasite and developmental form.</items>
      </items>
      <items code="at0004">
        <items id="text">Parasite name</items>
        <items id="description">Name of the parasite.</items>
        <items id="comment">Coding with an external terminology is recommended, if available. For example: Giardia, Entamoeba, Hookworm, Toxoplasma, Plasmodium or Trypanosoma</items>
      </items>
      <items code="at0005">
        <items id="text">Presence</items>
        <items id="description">Statement about detection of the identified parasite in the specimen.</items>
      </items>
      <items code="at0006">
        <items id="text">Detected</items>
        <items id="description"></items>
      </items>
      <items code="at0007">
        <items id="text">Not detected</items>
        <items id="description"></items>
      </items>
      <items code="at0008">
        <items id="text">Indeteminate</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the overall parasitology findings, not captured in other fields.</items>
      </items>
      <items code="at0010">
        <items id="text">Developmental form</items>
        <items id="description">Name of the developmental stage or lifecycle of the parasite.</items>
        <items id="comment">Coding with an external terminology is recommended, if available. For example: egg, cyst, larva, trophozoite.</items>
      </items>
      <items code="at0011">
        <items id="text">Qualitative result</items>
        <items id="description">Qualitative assessment of the amount of identified parasite in the specimen.</items>
      </items>
      <items code="at0012">
        <items id="text">Absent</items>
        <items id="description"></items>
      </items>
      <items code="at0013">
        <items id="text">Scant</items>
        <items id="description"></items>
      </items>
      <items code="at0014">
        <items id="text">Low</items>
        <items id="description"></items>
      </items>
      <items code="at0015">
        <items id="text">Moderate</items>
        <items id="description"></items>
      </items>
      <items code="at0016">
        <items id="text">High</items>
        <items id="description"></items>
      </items>
      <items code="at0017">
        <items id="text">Quantitative result</items>
        <items id="description">Quantitative assessment of the amount of identified parasite in the specimen.</items>
        <items id="comment">For example: 2% parasitaemia detected in a blood smear.</items>
      </items>
      <items code="at0018">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the findings for the identified parasite, not captured in other fields.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

