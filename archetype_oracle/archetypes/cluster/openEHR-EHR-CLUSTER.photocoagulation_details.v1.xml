  <ontology>
    <term_definitions language="es">
      <items code="at0000">
        <items id="text">Detalles acerca de fotocoagulación</items>
        <items id="description">Define los detalles específicos relativos a tratamientos oftálmicos que impliquen fotocoagulación.</items>
      </items>
      <items code="at0001">
        <items id="text">Lateralidad</items>
        <items id="description">Ojos en los cuales se aplica el tratamiento por fotocoagulación.</items>
      </items>
      <items code="at0002">
        <items id="text">Ojo izquierdo</items>
        <items id="description">Se observa el ojo izquierdo.</items>
      </items>
      <items code="at0003">
        <items id="text">Ojo derecho</items>
        <items id="description">Se observa el ojo derecho.</items>
      </items>
      <items code="at0004">
        <items id="text">Ambos ojos</items>
        <items id="description">La intervención se realiza en ambos ojos del paciente.</items>
      </items>
      <items code="at0005">
        <items id="text">Método fotocoagulación</items>
        <items id="description">Método elegido para llevar a cabo la intervención por fotocoagulación en la retina.</items>
      </items>
      <items code="at0006">
        <items id="text">Fotocoagulación panretiniana</items>
        <items id="description">**(en)</items>
      </items>
      <items code="at0007">
        <items id="text">Laser macular</items>
        <items id="description">Fotocoagulación de drusas maculares.</items>
      </items>
      <items code="at0008">
        <items id="text">Tipo de láser</items>
        <items id="description">Identifica el láser requerido para llevar a cabo la intervención.</items>
      </items>
      <items code="at0009">
        <items id="text">Láser de colorante</items>
        <items id="description">*</items>
      </items>
      <items code="at0010">
        <items id="text">Láser de gas</items>
        <items id="description">*</items>
      </items>
      <items code="at0011">
        <items id="text">Láser de diodo</items>
        <items id="description">*</items>
      </items>
    </term_definitions>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Photocoagulation details</items>
        <items id="description">Specific details concerning ophthalmic treatments using photocoagulation.</items>
      </items>
      <items code="at0001">
        <items id="text">Laterality</items>
        <items id="description">Eyes in which the treatment by photocoagulation is applied.</items>
      </items>
      <items code="at0002">
        <items id="text">Left eye</items>
        <items id="description">Left eye observation.</items>
      </items>
      <items code="at0003">
        <items id="text">Right eye</items>
        <items id="description">Right eye observation.</items>
      </items>
      <items code="at0004">
        <items id="text">Both eyes</items>
        <items id="description">Treatment performed on both eyes of the patient.</items>
      </items>
      <items code="at0005">
        <items id="text">Photocoagulation method</items>
        <items id="description">Method chosen to perform the treatment by photocoagulation within the retina.</items>
      </items>
      <items code="at0006">
        <items id="text">Panretinal photocoagulation</items>
        <items id="description">*</items>
      </items>
      <items code="at0007">
        <items id="text">Macular laser</items>
        <items id="description">Photocoagulation of macular drusen.</items>
      </items>
      <items code="at0008">
        <items id="text">Type of laser</items>
        <items id="description">Identifies the type of laser device needed to perform the treatment.</items>
      </items>
      <items code="at0009">
        <items id="text">Dye laser device</items>
        <items id="description">*</items>
      </items>
      <items code="at0010">
        <items id="text">Gas laser device</items>
        <items id="description">*</items>
      </items>
      <items code="at0011">
        <items id="text">Diode pumped laser device</items>
        <items id="description">*</items>
      </items>
    </term_definitions>
