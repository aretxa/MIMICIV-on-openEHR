  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Knowledge base reference</items>
        <items id="description">A citation of a digital resource used as an source of authoritative or expert information, and/or to items contained within the resource.</items>
        <items id="comment">For example: a genomics pipeline history, a database of genomic variants, or a database of healthcare procedure guidelines.</items>
      </items>
      <items code="at0001">
        <items id="text">Knowledge base name</items>
        <items id="description">The name of the knowledge base.</items>
        <items id="comment">For example: Galaxy/Snakemake; dbSNP; or CADD.</items>
      </items>
      <items code="at0002">
        <items id="text">Item version</items>
        <items id="description">The version of the referenced item within the knowledge base.</items>
      </items>
      <items code="at0003">
        <items id="text">Item URI</items>
        <items id="description">The hyperlink to the referenced item within the knowledge base.</items>
      </items>
      <items code="at0004">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the knowledge base, not captured in other fields.</items>
      </items>
      <items code="at0005">
        <items id="text">Item name</items>
        <items id="description">The name of the referenced item within the knowledge base.</items>
        <items id="comment">For example: rs139581412.</items>
      </items>
      <items code="at0006">
        <items id="text">Knowledge base version</items>
        <items id="description">The version of the referenced knowledge base.</items>
      </items>
      <items code="at0007">
        <items id="text">Item publication</items>
        <items id="description">Date and/or time when the item within the database was published.</items>
      </items>
      <items code="at0008">
        <items id="text">Knowledge base URI</items>
        <items id="description">The hyperlink to the referenced knowledge base.</items>
      </items>
    </term_definitions>
