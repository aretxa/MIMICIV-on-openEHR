  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Distribution</items>
        <items id="description">Details of the target of communication distribution, whether to identified individual parties or as a category.</items>
      </items>
      <items code="at0003">
        <items id="text">Communication mode</items>
        <items id="description">The communications mode by which the distribution is to be made.</items>
        <items id="comment">e.g. fax, email.</items>
      </items>
      <items code="at0006">
        <items id="text">Date sent</items>
        <items id="description">Date that the distribution was sent.</items>
      </items>
      <items code="at0007">
        <items id="text">Recipient details</items>
        <items id="description">An individual person or organisation to whom the distribution applies.</items>
      </items>
      <items code="at0008">
        <items id="text">Group category</items>
        <items id="description">A named category of group e.g. Social work, patient representatives to whom the distribution applies.</items>
      </items>
      <items code="at0010">
        <items id="text">Recipient identifier</items>
        <items id="description">Unique identifier for an individual recipient.</items>
      </items>
      <items code="at0011">
        <items id="text">Individual recipient</items>
        <items id="description">Distribution details for an individual recipient.</items>
      </items>
      <items code="at0012">
        <items id="text">Urgent</items>
        <items id="description">If true the notification should be distributed made as a matter of urgency.</items>
      </items>
    </term_definitions>
    <term_definitions language="sl">
      <items code="at0000">
        <items id="text">Obveščanje</items>
        <items id="description">Seznam prejemnikov - skupina posameznikov, ki so naslovniki sporočila</items>
      </items>
      <items code="at0003">
        <items id="text">Način komunikacija</items>
        <items id="description">Način obveščanja</items>
      </items>
      <items code="at0006">
        <items id="text">Datum odpošiljanja</items>
        <items id="description">Datum obveščanja - distribuiranja sporočila</items>
      </items>
      <items code="at0007">
        <items id="text">Podrobnosti o prejemniku</items>
        <items id="description">Posameznik ali organizacija, na koga je naslovljeno sporočilo</items>
      </items>
      <items code="at0008">
        <items id="text">Skupina</items>
        <items id="description">Ime skupine prejemnikov, npr. 'zdravniki predstojniki oddelkov'</items>
      </items>
      <items code="at0010">
        <items id="text">ID prejemnika</items>
        <items id="description">Unikatni identifikator posameznika (primarni ključ, ki določa posameznika)</items>
      </items>
      <items code="at0011">
        <items id="text">Posamezni prejemnik</items>
        <items id="description">Podatki o posameznem prejemniku</items>
      </items>
      <items code="at0012">
        <items id="text">Nujno</items>
        <items id="description">Če potrebna nujna obravnava?</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Distribusjon</items>
        <items id="description">Detaljer om den målgruppen som meddelelsen skal distribueres til, enten for identifikasjon av et individuelle parter eller som kategori.</items>
      </items>
      <items code="at0003">
        <items id="text">Kommunikasjons modus</items>
        <items id="description">Den metode som anvendes til distribusjon av meddelelsen.</items>
        <items id="comment">fx fax, e-post, brev.</items>
      </items>
      <items code="at0006">
        <items id="text">Forsendelse dato</items>
        <items id="description">Dato for utsending av meddelelse.</items>
      </items>
      <items code="at0007">
        <items id="text">Mottaker dealjer</items>
        <items id="description">En individuell person eller organisasjon som meddelelsen vedrører.</items>
      </items>
      <items code="at0008">
        <items id="text">Gruppe kategori</items>
        <items id="description">En navngitt gruppe fx sosialt arbeide, pasient representanter som meddelelsen vedrører.</items>
      </items>
      <items code="at0010">
        <items id="text">Mottaker identifikator</items>
        <items id="description">Unik identifikator for en individuell mottaker.</items>
      </items>
      <items code="at0011">
        <items id="text">Individuell mottaker</items>
        <items id="description">Distribusjons detaljer for en individuell mottaker.</items>
      </items>
      <items code="at0012">
        <items id="text">Haster</items>
        <items id="description">Hvis sant: meddelelsen bør sendes som hastemeddelelse.</items>
      </items>
    </term_definitions>
  </ontology>
