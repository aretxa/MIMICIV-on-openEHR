  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Deep tendon reflex response</items>
        <items id="description">Findings observed during the physical examination of a single deep tendon reflex.</items>
      </items>
      <items code="at0026">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the reflex response findings, not captured in other fields.</items>
      </items>
      <items code="at0001">
        <items id="text">Reflex</items>
        <items id="description">Identification of the deep tendon reflex or group of reflexes being examined.</items>
      </items>
      <items code="at0013">
        <items id="text">Relative response</items>
        <items id="description">The elicited reflex response represented as relative to a 'normal' response.</items>
      </items>
      <items code="at0015">
        <items id="text">Normal</items>
        <items id="description">The elicited response is considered within the expected range of responses.</items>
      </items>
      <items code="at0014">
        <items id="text">Increased</items>
        <items id="description">The elicited response appears increased, compared with normal.</items>
      </items>
      <items code="at0016">
        <items id="text">Decreased</items>
        <items id="description">The elicited response appears decreased, compared with normal.</items>
      </items>
      <items code="at0025">
        <items id="text">Augmented response</items>
        <items id="description">Was the response augmented or reinforced?</items>
      </items>
      <items code="at0002">
        <items id="text">Right biceps</items>
        <items id="description">Testing the C5 and C6 nerve roots.</items>
      </items>
      <items code="at0004">
        <items id="text">Right brachioradialis</items>
        <items id="description">Testing the C6 nerve root.</items>
      </items>
      <items code="at0006">
        <items id="text">Right triceps</items>
        <items id="description">Testing the C7 nerve root.</items>
      </items>
      <items code="at0007">
        <items id="text">Left triceps</items>
        <items id="description">Testing the C7 nerve root.</items>
      </items>
      <items code="at0008">
        <items id="text">Right quadriceps</items>
        <items id="description">Also known as patellar or knee jerk. Testing the L4 nerve root.</items>
      </items>
      <items code="at0010">
        <items id="text">Right achilles</items>
        <items id="description">Also known as ankle jerk. Testing the S1 nerve root.</items>
      </items>
      <items code="at0012">
        <items id="text">Jaw</items>
        <items id="description">Testing the 5th cranial nerve root.</items>
      </items>
      <items code="at0003">
        <items id="text">Left biceps</items>
        <items id="description">Testing the C5 and C6 nerve roots.</items>
      </items>
      <items code="at0005">
        <items id="text">Left brachioradialis</items>
        <items id="description">Testing the C6 nerve root.</items>
      </items>
      <items code="at0009">
        <items id="text">Left quadriceps</items>
        <items id="description">Also known as patellar or knee jerk. Testing the L4 nerve root.</items>
      </items>
      <items code="at0011">
        <items id="text">Left achilles</items>
        <items id="description">Also known as ankle jerk. Testing the S1 nerve root.</items>
      </items>
      <items code="at0018">
        <items id="text">Graded response</items>
        <items id="description">The elicited reflex response represented as a grade.</items>
      </items>
      <items code="at0019">
        <items id="text">Absent</items>
        <items id="description">Reflex is not evident. May be recorded as '0' or '-'.</items>
      </items>
      <items code="at0020">
        <items id="text">Slight</items>
        <items id="description">Reflex is present but less than normal amplitude, or only seen with reinforcement. May be recorded as '+' or '1+'.</items>
      </items>
      <items code="at0021">
        <items id="text">Normal</items>
        <items id="description">Reflex is present at normal amplitude. May be recorded as '++' or '2+'.</items>
      </items>
      <items code="at0022">
        <items id="text">Brisk</items>
        <items id="description">Reflex is present at greater than normal amplitude. May be recorded as '+++' or '3+'.</items>
      </items>
      <items code="at0023">
        <items id="text">Non-sustained clonus</items>
        <items id="description">Reflex is repeating but not sustained. May be recorded as '++++' or '4+'.</items>
      </items>
      <items code="at0024">
        <items id="text">Sustained clonus</items>
        <items id="description">Reflex is repeating and sustained. May be recorded as '+++++' or '5+'.</items>
      </items>
      <items code="at0017">
        <items id="text">Absent</items>
        <items id="description">No response can be elicited.</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

