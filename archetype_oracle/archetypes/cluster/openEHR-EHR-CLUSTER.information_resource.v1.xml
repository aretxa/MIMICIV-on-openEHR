  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Information resource</items>
        <items id="description">Information, instructional or educational material supplied to an individual, or their carer, as part of the provision of healthcare.</items>
      </items>
      <items code="at0001">
        <items id="text">Content</items>
        <items id="description">Digital representation of the information resource.</items>
        <items id="comment">The actual content provided can be captured and stored using the Multimedia data type. For example: RTF or PDF for a document; JPG for an image; MP4 for a video; or WAV for an audio file. If the content is located elsewhere the external location can be referenced using the URI data type.</items>
      </items>
      <items code="at0002">
        <items id="text">Resource name</items>
        <items id="description">Name or title of the information resource.</items>
        <items id="comment">For example: the name of the pamphlet; the title of a book.</items>
      </items>
      <items code="at0005">
        <items id="text">Description</items>
        <items id="description">Narrative description about the information resource.</items>
        <items id="comment">For example: Description of the content, place published and publisher.</items>
      </items>
      <items code="at0007">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the information resource not captured in other fields.</items>
      </items>
      <items code="at0010">
        <items id="text">Identifier</items>
        <items id="description">Identifier for the information resource.</items>
        <items id="comment">For example: PMID (PubMed Identifier) or DOI (Digital Object Identifier).</items>
      </items>
      <items code="at0012">
        <items id="text">Author</items>
        <items id="description">Details about the individual or organisation who created or authored the information resource.</items>
      </items>
      <items code="at0013">
        <items id="text">Additional details</items>
        <items id="description">Further details about the information resource.</items>
      </items>
      <items code="at0014">
        <items id="text">Published</items>
        <items id="description">The date or period when the information resource was published or released.</items>
        <items id="comment">Partial dates are permitted.</items>
      </items>
      <items code="at0015">
        <items id="text">Version</items>
        <items id="description">The version or edition of the information resource.</items>
      </items>
      <items code="at0016">
        <items id="text">Type of media</items>
        <items id="description">The type of material provided.</items>
        <items id="comment">For example: a book; brochure; patient-education leaflet; website; mobile application; video; or podcast.</items>
      </items>
    </term_definitions>
