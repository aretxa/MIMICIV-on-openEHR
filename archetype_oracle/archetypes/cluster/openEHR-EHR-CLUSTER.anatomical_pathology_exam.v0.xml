  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Anatomical pathology examination</items>
        <items id="description">Findings and interpretation of an anatomical pathology examination performed on tissues and body fluids.</items>
      </items>
      <items code="at0005">
        <items id="text">Anatomical pathology finding</items>
        <items id="description">Details of an individual anatomical pathology finding, often related to a specific anatomical location or specimen.</items>
      </items>
      <items code="at0006">
        <items id="text">Tissue available</items>
        <items id="description">True if the tissue is available for examination.</items>
      </items>
      <items code="at0008">
        <items id="text">Finding description</items>
        <items id="description">A narrative description of the anatomical pathology finding.</items>
      </items>
      <items code="at0009">
        <items id="text">Structured findings</items>
        <items id="description">Detailed structured findings.</items>
      </items>
      <items code="at0010">
        <items id="text">Pathology interpretation</items>
        <items id="description">Single word, phrase of brief description representing the interpretation of the anatomical pathology finding. A coded term is preferred.</items>
      </items>
      <items code="at0011">
        <items id="text">Examination description</items>
        <items id="description">A narrative description of the entire anatomical pathology examination.</items>
      </items>
      <items code="at0017">
        <items id="text">Specimen container ID</items>
        <items id="description">Reference ID, URI or text for a specimen container related to this finding.</items>
      </items>
      <items code="at0018">
        <items id="text">Specimen container ID</items>
        <items id="description">Reference ID, URI or text for a specimen containera related to this finding.</items>
      </items>
      <items code="at0019">
        <items id="text">Examination type</items>
        <items id="description">Identification of the type of anatomical pathology examination performed.</items>
      </items>
      <items code="at0020">
        <items id="text">Macroscopic examination</items>
        <items id="description">Findings recorded on examination of a gross specimen.</items>
      </items>
      <items code="at0021">
        <items id="text">Microscopic examination</items>
        <items id="description">Findings recorded after microsopic examination.</items>
      </items>
      <items code="at0022">
        <items id="text">Multimedia representation</items>
        <items id="description">Digital image, video or diagram representing the examination.</items>
        <items id="comment">Multiple formats are allowed but they should represent equivalent clinical content.</items>
      </items>
      <items code="at0023">
        <items id="text">Finding label</items>
        <items id="description">A text label for the specific finding.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Patologiundersøkelse</items>
        <items id="description">Funn og tolkninger av patologiundersøkelser utført på vev, celler og kroppsvæsker.</items>
      </items>
      <items code="at0005">
        <items id="text">Patologifunn</items>
        <items id="description">Detaljer om et individuelt patologifunn, ofte relatert til en spesifikk anatomisk lokalisering eller prøvemateriale.</items>
      </items>
      <items code="at0006">
        <items id="text">Vev tilgjengelig</items>
        <items id="description">Sann dersom vevet er tilgjengelig for undersøkelse.</items>
      </items>
      <items code="at0008">
        <items id="text">Funnbeskrivelse</items>
        <items id="description">Fritekstbeskrivelse av patologifunnet.</items>
      </items>
      <items code="at0009">
        <items id="text">Strukturerte funn</items>
        <items id="description">Detaljerte strukturerte funn.</items>
      </items>
      <items code="at0010">
        <items id="text">Tolkning</items>
        <items id="description">Enkeltord, frase eller kort beskrivelse som representerer tolkningen av patologifunnet. Koding med en terminologi foretrekkes.</items>
      </items>
      <items code="at0011">
        <items id="text">Undersøkelsesbeskrivelse</items>
        <items id="description">Fritekstbeskrivelse av patologiundersøkelsen.</items>
      </items>
      <items code="at0017">
        <items id="text">Prøvebeholder-ID</items>
        <items id="description">Referanse ID, URI eller tekst til en prøvebeholder relatert til dette funnet.</items>
      </items>
      <items code="at0018">
        <items id="text">Prøvebeholder-ID</items>
        <items id="description">Referanse ID, URI eller tekst til en prøvebeholder relatert til denne undersøkelsen.</items>
      </items>
      <items code="at0019">
        <items id="text">Undersøkelsestype</items>
        <items id="description">Spesifisering av hvilken type patologiundersøkelse som er utført.</items>
      </items>
      <items code="at0020">
        <items id="text">Makroskopisk undersøkelse</items>
        <items id="description">Funn er basert på en makroskopisk undersøkelse.</items>
      </items>
      <items code="at0021">
        <items id="text">Mikroskopisk undersøkelse</items>
        <items id="description">Funn er basert på en mikroskopisk undersøkelse.</items>
      </items>
      <items code="at0022">
        <items id="text">Multimediarepresentasjon</items>
        <items id="description">Digitalt bilde, video eller diagram som representerer undersøkelsen.</items>
        <items id="comment">Flere formater tillates, men innholdet i de forskjellige formatene må representere det samme innholdet.</items>
      </items>
      <items code="at0023">
        <items id="text">Funnavn</items>
        <items id="description">Tekstlig navn på det spesifikke funnet.</items>
      </items>
    </term_definitions>
    <term_definitions language="de">
      <items code="at0000">
        <items id="text">Pathologisch-anatomische Begutachtung</items>
        <items id="description">Befunde und Interpretationen von pathologisch-anatomischen Begutachtungen (Histopathologie) an Gewebe und Körperflüssigkeiten.</items>
      </items>
      <items code="at0005">
        <items id="text">Pathologisch-anatomischer Befund</items>
        <items id="description">Details zu einem einzelnen pathologisch-anatomischen Befund, oft im Bezug zu einer spezifischen anatomischen Lokalisation oder Probe.</items>
      </items>
      <items code="at0006">
        <items id="text">Gewebe vorhanden</items>
        <items id="description">Ist "wahr", wenn das Gewebe für die Untersuchung zur Verfügung steht.</items>
      </items>
      <items code="at0008">
        <items id="text">Befundbeschreibung</items>
        <items id="description">Eine Beschreibung des pathologisch-anatomischen Befunds.</items>
      </items>
      <items code="at0009">
        <items id="text">Strukturierte Befunde</items>
        <items id="description">Detaillierte strukturierte Befunde.</items>
      </items>
      <items code="at0010">
        <items id="text">Pathologische Interpretation</items>
        <items id="description">Ein Wort oder ein kurzer Satz, der eine Interpretation des pathologisch-anatomischen Befundes beschreibt. Eine kodierte Erfassung wird bevorzugt.</items>
      </items>
      <items code="at0011">
        <items id="text">Untersuchungsbefund</items>
        <items id="description">Eine Beschreibung der gesamten anatomisch-pathologischen Begutachtung.</items>
      </items>
      <items code="at0017">
        <items id="text">Probenbehälter ID</items>
        <items id="description">Referenz-ID, URI oder Text der Probe für diesen Befund.  </items>
      </items>
      <items code="at0018">
        <items id="text">Probenbehälter ID</items>
        <items id="description">Referenz-ID, URI oder Text der Probe für diesen Befund.  </items>
      </items>
      <items code="at0019">
        <items id="text">Art der Untersuchung</items>
        <items id="description">Identifizierung der Art der durchgeführten anatomisch-pathologischen Untersuchung.</items>
      </items>
      <items code="at0020">
        <items id="text">Makroskopische Untersuchung</items>
        <items id="description">Befunde aus einer makroskopischen Gewebeprobe.</items>
      </items>
      <items code="at0021">
        <items id="text">Mikroskopische Untersuchung</items>
        <items id="description">Befunde aus einer mikroskopischen Gewebeuntersuchung.</items>
      </items>
      <items code="at0022">
        <items id="text">Testrepräsentation</items>
        <items id="description">Rich Text oder eine andere multimediale Darstellung eines speziellen Ergebnisses, ermittelt durch einen Diagnosedienst.</items>
        <items id="comment">Es sind mehrere Formatangaben erlaubt, diese müssen aber semantisch äquivalent sein.</items>
      </items>
      <items code="at0023">
        <items id="text">Befundbezeichnung</items>
        <items id="description">Bezeichnung für den konkreten Befund.&#xD;
</items>
      </items>
    </term_definitions>
  </ontology>
</archetype>

