  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Coronary artery stenosis</items>
        <items id="description">Details of coronary arterial calcific plaque and the situation of stenosis.</items>
      </items>
      <items code="at0001">
        <items id="text">Stenosis location</items>
        <items id="description">A standardized approach to coronary segmentation.</items>
      </items>
      <items code="at0004">
        <items id="text">Stenosis severity</items>
        <items id="description">Quantitative assessment of stenosis severity.</items>
      </items>
      <items code="at0005">
        <items id="text">Lesion length</items>
        <items id="description">Quantitative assessment of stenosis severity.</items>
      </items>
      <items code="at0006">
        <items id="text">Plaque type</items>
        <items id="description">The type of detected plaque.</items>
      </items>
      <items code="at0007">
        <items id="text">Modifier</items>
        <items id="description">To indicate that a study is not fully evaluable or non-diagnostic (N) or to indicate the presence of stents (S), grafts (G), and vulnerable plaque (V).</items>
      </items>
      <items code="at0008">
        <items id="text">Comment</items>
        <items id="description">To describe uninterpretable segments.</items>
      </items>
      <items code="at0015">
        <items id="text">Normal</items>
        <items id="description">absence of plaque and no luminal stenosis</items>
      </items>
      <items code="at0016">
        <items id="text">Minimal</items>
        <items id="description">plaque with &lt;25% stenosis</items>
      </items>
      <items code="at0017">
        <items id="text">Mild</items>
        <items id="description">25% to 49% stenosis</items>
      </items>
      <items code="at0018">
        <items id="text">Moderate</items>
        <items id="description">50% to 69% stenosis</items>
      </items>
      <items code="at0019">
        <items id="text">Severe</items>
        <items id="description">70% to 99% stenosis</items>
      </items>
      <items code="at0020">
        <items id="text">Occluded</items>
        <items id="description">apparent occlusion</items>
      </items>
      <items code="at0021">
        <items id="text">Proximal RCA (pRCA)</items>
        <items id="description">Ostium of the RCA(right coronary artery) to one-half the distance to the acute margin of heart.</items>
      </items>
      <items code="at0022">
        <items id="text">Mid RCA (mRCA)</items>
        <items id="description">End of proximal RCA to the acute margin of heart.</items>
      </items>
      <items code="at0023">
        <items id="text">Distal RCA (dRCA)</items>
        <items id="description">End of mid RCA to origin of the PDA (posterior descending artery)</items>
      </items>
      <items code="at0024">
        <items id="text">PDA-R (R-PDA)</items>
        <items id="description">PDA from RCA.</items>
      </items>
      <items code="at0025">
        <items id="text">Left main (LM)</items>
        <items id="description">Ostium of LM to bifurcation of LAD (left anterior descending artery) and LCx (left circumflex artery).</items>
      </items>
      <items code="at0026">
        <items id="text">Proximal LAD (pLAD)</items>
        <items id="description">End of LM to the first large septal or D1 (first diagonal; &gt;1.5 mm in size) whichever is most proximal.</items>
      </items>
      <items code="at0027">
        <items id="text">Mid LAD (mLAD)</items>
        <items id="description">End of proximal LAD to one-half the distance to the apex.</items>
      </items>
      <items code="at0028">
        <items id="text">Distal LAD (dLAD)</items>
        <items id="description">End of mid LAD to end of LAD.</items>
      </items>
      <items code="at0029">
        <items id="text">D1</items>
        <items id="description">First diagonal branch D1.</items>
      </items>
      <items code="at0030">
        <items id="text">D2</items>
        <items id="description">Second diagonal branch D2.</items>
      </items>
      <items code="at0031">
        <items id="text">Proximal LCx (pCx)</items>
        <items id="description">End of LM to the origin of the OM1 (first obtuse marginal).</items>
      </items>
      <items code="at0032">
        <items id="text">OM1</items>
        <items id="description">First OM1 traversing the lateral wall of the left ventricle.</items>
      </items>
      <items code="at0033">
        <items id="text">Mid and distal LCx (LCx)</items>
        <items id="description">Traveling in the atrioventricular groove, distal to the OM1 branch to the end of the vessel or origin of the L-PDA (left PDA).</items>
      </items>
      <items code="at0034">
        <items id="text">OM2</items>
        <items id="description">Second marginal OM2.</items>
      </items>
      <items code="at0035">
        <items id="text">PDA-L (L-PDA)</items>
        <items id="description">PDA from LCx.</items>
      </items>
      <items code="at0036">
        <items id="text">PLB-R (R-PLB)</items>
        <items id="description">PLB(posterior-lateral branch) from RCA.</items>
      </items>
      <items code="at0037">
        <items id="text">Ramus intermedius (RI)</items>
        <items id="description">Vessel originating from the left main between the LAD and LCx in case of a trifurcation.</items>
      </items>
      <items code="at0038">
        <items id="text">PLB-L (L-PLB)</items>
        <items id="description">PLB from LCx.</items>
      </items>
      <items code="at0039">
        <items id="text">Discrete</items>
        <items id="description">The lesion length is below 10mm.</items>
      </items>
      <items code="at0040">
        <items id="text">Tubular</items>
        <items id="description">The lesion length is between 10-20mm.</items>
      </items>
      <items code="at0041">
        <items id="text">Diffuse</items>
        <items id="description">The lesion length is above 20mm.</items>
      </items>
      <items code="at0042">
        <items id="text">S</items>
        <items id="description">To indicate the presence of stents.</items>
      </items>
      <items code="at0043">
        <items id="text">V</items>
        <items id="description">To indicate vulnerable plaque.</items>
      </items>
      <items code="at0044">
        <items id="text">N</items>
        <items id="description">Not fully evaluable or non-diagnostic.</items>
      </items>
      <items code="at0045">
        <items id="text">G</items>
        <items id="description">To indicate grafts.</items>
      </items>
      <items code="at0046">
        <items id="text">Calcium score [Cluster]</items>
        <items id="description"></items>
      </items>
      <items code="at0047">
        <items id="text">Presence of plaque</items>
        <items id="description"></items>
      </items>
      <items code="at0048">
        <items id="text">Vulnerable plaque assessment</items>
        <items id="description"></items>
      </items>
    </term_definitions>
  </ontology>
