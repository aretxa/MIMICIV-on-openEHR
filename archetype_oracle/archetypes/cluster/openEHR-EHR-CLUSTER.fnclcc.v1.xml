  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">FNCLCC grading system</items>
        <items id="description">The histological grading of soft tissue sarcoma using the FNCLCC grading system.</items>
        <items id="comment">FNCLCC - Fédération Nationale des Centres de Lutte Contre le Cancer.</items>
      </items>
      <items code="at0005">
        <items id="text">Tumour differentiation</items>
        <items id="description"></items>
      </items>
      <items code="at0006">
        <items id="text">Mitotic count</items>
        <items id="description"></items>
      </items>
      <items code="at0007">
        <items id="text">Tumour necrosis</items>
        <items id="description"></items>
      </items>
      <items code="at0008">
        <items id="text">Sarcoma closely resembling normal adult mesenchymal tissue (e.g., low-grade liposarcoma).</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">Sarcomas for which histological typing is certain (e.g., myxoid/round cell liposarcoma).</items>
        <items id="description"></items>
      </items>
      <items code="at0010">
        <items id="text">Embryonal and undifferentiated sarcomas, sarcomas of doubtful type, synovial sarcomas, soft tissue osteosarcoma, Ewing sarcoma/primitive neuroectodermal tumor (PNET) of soft tissue.</items>
        <items id="description"></items>
      </items>
      <items code="at0011">
        <items id="text">0-9 mitoses per 1,7 mm2</items>
        <items id="description"></items>
      </items>
      <items code="at0012">
        <items id="text">10-19 mitoses per 1.7 mm2</items>
        <items id="description"></items>
      </items>
      <items code="at0013">
        <items id="text">≥20 mitoses per 1.7 mm2</items>
        <items id="description"></items>
      </items>
      <items code="at0014">
        <items id="text">No necrosis</items>
        <items id="description"></items>
      </items>
      <items code="at0015">
        <items id="text">&lt;50% tumor necrosis</items>
        <items id="description"></items>
      </items>
      <items code="at0016">
        <items id="text">≥50% tumor necrosis</items>
        <items id="description"></items>
      </items>
      <items code="at0017">
        <items id="text">Histological grade</items>
        <items id="description">Assessment of histological grade based on the total score in the FNCLCC grading system.</items>
        <items id="comment">In clinical practice for soft tissue sarcomas, the 'Histological grade' result from this archetype is commonly reused to populate the 'Histopathological grade (G)' data element within the CLUSTER.tnm and CLUSTER.tnm-pathological archetypes. In that situation, this grading may be recorded within this archetype and also copied to the TNM archetypes.</items>
      </items>
      <items code="at0018">
        <items id="text">Grade 1</items>
        <items id="description">Total score 2 or 3.</items>
      </items>
      <items code="at0019">
        <items id="text">Grade 2</items>
        <items id="description">Total score 4 or 5.</items>
      </items>
      <items code="at0020">
        <items id="text">Grade 3</items>
        <items id="description">Total score 6, 7 or 8.</items>
      </items>
      <items code="at0021">
        <items id="text">Total score</items>
        <items id="description">The total sum of each component parameter for the FNCLCC grading system.</items>
      </items>
      <items code="at0022">
        <items id="text">GX</items>
        <items id="description">Grade cannot be assessed.</items>
      </items>
    </term_definitions>
