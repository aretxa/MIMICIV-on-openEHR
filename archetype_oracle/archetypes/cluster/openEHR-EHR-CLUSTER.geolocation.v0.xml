  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Geolocation</items>
        <items id="description">A geographical place on the surface of the earth, specified by latitude, longitude and height or depth.</items>
      </items>
      <items code="at0001">
        <items id="text">Latitude</items>
        <items id="description">Horizontal (y) coordinate.</items>
        <items id="comment">Latitude is represented by positive values north of the equator and negative values south of the equator. For example the latitude of the Eiffel Tower is 48.858093.</items>
      </items>
      <items code="at0002">
        <items id="text">Longitude</items>
        <items id="description">Horizontal (x) coordinate.</items>
        <items id="comment">Longitude is represented by negative values west of Prime Meridian and positive values east of Prime Meridian. For example the longitude of the Eiffel Tower is 2.294694</items>
      </items>
      <items code="at0003">
        <items id="text">Altitude</items>
        <items id="description">The vertical coordinate representing height or depth.</items>
        <items id="comment">For example, the height of the Eiffel Tower is 324m to the tip.</items>
      </items>
    </term_definitions>
  </ontology>
