  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Endotracheal tube (ETT)</items>
        <items id="description">Details about a flexible plastic tube that is inserted inside the trachea to secure an airway.</items>
        <items id="comment">Most typically used as part of a breathing system in anaesthesia or mechanical ventilation.</items>
      </items>
      <items code="at0001">
        <items id="text">Type??? Cuffed?</items>
        <items id="description">The type of ETT.</items>
      </items>
      <items code="at0002">
        <items id="text">Cuffed</items>
        <items id="description"></items>
      </items>
      <items code="at0003">
        <items id="text">Uncuffed</items>
        <items id="description"></items>
      </items>
      <items code="at0004">
        <items id="text">?Manufactured length</items>
        <items id="description">The measured length of the lumen at manufacture.</items>
      </items>
      <items code="at0005">
        <items id="text">Radiopaque</items>
        <items id="description">Does the ETT contain radiopaque material?</items>
        <items id="comment">Allows identification of the tube tip position on Xray.</items>
      </items>
      <items code="at0006">
        <items id="text">Connector size</items>
        <items id="description">The size of the connection between the ETT and the breathing system.</items>
      </items>
      <items code="at0007">
        <items id="text">15 mm</items>
        <items id="description"></items>
      </items>
      <items code="at0008">
        <items id="text">22 mm</items>
        <items id="description"></items>
      </items>
      <items code="at0009">
        <items id="text">Murphy's eye</items>
        <items id="description">Presence of an alternate gas passage in case the ETT tip becomes occluded.</items>
      </items>
      <items code="at0012">
        <items id="text">Per lumen</items>
        <items id="description">Details relevant to a single lumen.</items>
      </items>
      <items code="at0013">
        <items id="text">Internal diameter</items>
        <items id="description">The measured internal diameter of the lumen.</items>
      </items>
      <items code="at0014">
        <items id="text">Cuff type</items>
        <items id="description"></items>
      </items>
      <items code="at0015">
        <items id="text">?Label/Lumen name</items>
        <items id="description">Unique name to identify the lumen.</items>
      </items>
      <items code="at0016">
        <items id="text">Insertion site</items>
        <items id="description">The body site where the ETT enters the body.</items>
      </items>
      <items code="at0017">
        <items id="text">Oral</items>
        <items id="description"></items>
      </items>
      <items code="at0018">
        <items id="text">Nasal</items>
        <items id="description"></items>
      </items>
      <items code="at0019">
        <items id="text">Tracheostomy</items>
        <items id="description"></items>
      </items>
      <items code="at0020">
        <items id="text">8.5mm</items>
        <items id="description"></items>
      </items>
      <items code="at0021">
        <items id="text">30mm</items>
        <items id="description"></items>
      </items>
      <items code="at0022">
        <items id="text">Connector type</items>
        <items id="description">The type of connection between the ETT and the breathing system.</items>
        <items id="comment">For example: push fit; Luerlock.</items>
      </items>
      <items code="at0023">
        <items id="text">Number of cuffs (/??lumen) Is there ever one cuff with 2 lumen?</items>
        <items id="description">The number of cuffs manufactured.</items>
      </items>
      <items code="at0024">
        <items id="text">Laser resistant</items>
        <items id="description">Is the ETT manufactured from laser resistant material?</items>
      </items>
      <items code="at0025">
        <items id="text">External diameter</items>
        <items id="description">The measured external diameter of the lumen.</items>
      </items>
      <items code="at0026">
        <items id="text">Cuff position</items>
        <items id="description">The location of the cuff in situ.</items>
      </items>
      <items code="at0027">
