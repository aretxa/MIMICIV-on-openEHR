  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Laboratory stain findings</items>
        <items id="description">Findings observed from the examination of tissue treated with specialised stains intended to enhance the visibility of tissue structures and cellular details under a microscope.</items>
      </items>
      <items code="at0004">
        <items id="text">Stain name</items>
        <items id="description">Name of the technique used to stain the specimen.</items>
      </items>
      <items code="at0005">
        <items id="text">Gram stain</items>
        <items id="description"></items>
      </items>
      <items code="at0006">
        <items id="text">India ink stain</items>
        <items id="description"></items>
      </items>
      <items code="at0007">
        <items id="text">Ziehl-Neelsen stain</items>
        <items id="description"></items>
      </items>
      <items code="at0008">
        <items id="text">Description</items>
        <items id="description">Narrative description about the findings observed on microscopic inspection of a specimen using the identified stain.</items>
      </items>
      <items code="at0009">
        <items id="text">Finding</items>
        <items id="description">Microscopic finding observed using the identified stain.</items>
      </items>
      <items code="at0010">
        <items id="text">Positive</items>
        <items id="description"></items>
      </items>
      <items code="at0011">
        <items id="text">Negative</items>
        <items id="description"></items>
      </items>
      <items code="at0012">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the microscopy findings using the identified stain, not captured in other fields.</items>
      </items>
    </term_definitions>
  </ontology>
