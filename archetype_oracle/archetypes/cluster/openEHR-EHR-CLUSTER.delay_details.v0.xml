  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Delay details</items>
        <items id="description">Information about subcategories of delay or specific delays in seeking, reaching or receiving healthcare using the "Three Delays" framework.</items>
      </items>
      <items code="at0001">
        <items id="text">Delay name</items>
        <items id="description">The name of one or more subcategories of delay or of a specific delay.</items>
      </items>
      <items code="at0002">
        <items id="text">Delay presence?</items>
        <items id="description">Was the delay recognised as present, relevant to the identified 'Delay name'?</items>
      </items>
      <items code="at0003">
        <items id="text">Yes</items>
        <items id="description"></items>
      </items>
      <items code="at0004">
        <items id="text">No</items>
        <items id="description"></items>
      </items>
      <items code="at0005">
        <items id="text">Unknown</items>
        <items id="description"></items>
      </items>
      <items code="at0006">
        <items id="text">Description</items>
        <items id="description">Narrative description about the identified delay.</items>
      </items>
      <items code="at0007">
        <items id="text">Delay duration</items>
        <items id="description">The duration of the recognised delay.</items>
      </items>
      <items code="at0011">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the identified delay, not captured in other fields.</items>
      </items>
      <items code="at0010">
        <items id="text">Additional details</items>
        <items id="description">Additional details about the identified delay.</items>
        <items id="comment">This SLOT allows another instance of this CLUSTER.delay_detail archetype to be nested, to support representation of multiple levels of delay.</items>
      </items>
      <items code="at0009">
        <items id="text">Solution responsibility</items>
        <items id="description">The name of the person or organisation assigned responsibility to solve or mitigate the identified delay.</items>
      </items>
      <items code="at0008">
        <items id="text">Solution timeline</items>
        <items id="description">The due date or proposed timeframe for resolution or mitigation of the identified delay.</items>
      </items>
    </term_definitions>
  </ontology>
