        <items id="text">Body site</items>
        <items id="description">Identification of a single physical site either on, or within the human body.</items>
      </items>
      <items code="at0003">
        <items id="text">Structured body site</items>
        <items id="description">Additional detail using specific region or a point on , or within the identified body site.</items>
      </items>
