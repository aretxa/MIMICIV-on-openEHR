  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Oocyte specimen</items>
        <items id="description">To document detailed information about one or more harvested oocytes.</items>
      </items>
      <items code="at0018">
        <items id="text">Oocyte maturation stage</items>
        <items id="description">Nuclear maturity of oocyte after removal of the cumulus-corona cell mass.</items>
      </items>
      <items code="at0019">
        <items id="text">Metaphase 2</items>
        <items id="description">Oocyte in the metaphase 2 (M2) of the meiotic cell division.</items>
      </items>
      <items code="at0020">
        <items id="text">Meiosis 1</items>
        <items id="description">Oocyte in meiosis 1.</items>
      </items>
      <items code="at0021">
        <items id="text">Germinal vesicle</items>
        <items id="description">Oocyte still displaying an intact nucleus, the germinal vesicle (GV), in prophase 1 of meiosis.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Oocytt prøvemateriale</items>
        <items id="description">For å registrere detaljert informasjon om en eller flere uthentede oocytter.</items>
      </items>
      <items code="at0018">
        <items id="text">Modenhet av oocytt</items>
        <items id="description">Kjernens status i oocytten etter fjerning av cumulus-corona radiata celler.</items>
      </items>
      <items code="at0019">
        <items id="text">Metafase 2 [</items>
        <items id="description">Oocytt i metafase 2 (M2) av den meiotiske celledelingen.</items>
      </items>
      <items code="at0020">
        <items id="text">Meiose 1</items>
        <items id="description">Oocytt i meiose 1.</items>
      </items>
      <items code="at0021">
        <items id="text">Germinal vesikkel</items>
        <items id="description">Oocytt med en intakt kjerne, germinal vesikkel (GV), i profase 1 av den meiotiske celledelingen.</items>
      </items>
    </term_definitions>
  </ontology>
