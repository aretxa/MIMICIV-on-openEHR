  <ontology>
    <term_definitions language="en">
      <items code="at0018">
        <items id="text">Reference sequence</items>
        <items id="description">A sequence file that is used as a reference to describe genetic variants that are present in an analysed sequence.</items>
      </items>
      <items code="at0019">
        <items id="text">Source name</items>
        <items id="description">The name of the data source containing the reference sequence.</items>
      </items>
      <items code="at0020">
        <items id="text">Accession number</items>
        <items id="description">A unique identifier to refer to a sequence record in a sequence repository.</items>
      </items>
      <items code="at0021">
        <items id="text">Version number</items>
        <items id="description">The version number of the data record of the reference sequence.</items>
        <items id="comment">For example: 'hg38', 'hg19'.</items>
      </items>
      <items code="at0022">
