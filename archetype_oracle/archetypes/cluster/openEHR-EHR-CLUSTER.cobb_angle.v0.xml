  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Cobb angle</items>
        <items id="description">A measurement to determine the degree of curvature of the spine.</items>
      </items>
      <items code="at0002">
        <items id="text">Cobb angle</items>
        <items id="description">Measured angle of the spinal curve.</items>
      </items>
      <items code="at0010">
        <items id="text">Upper vertebra</items>
        <items id="description">The most tilted vertebra on the top of a spinal curve.</items>
        <items id="comment">Coding of the 'Upper vertebra' with a terminology is preferred, where possible.</items>
      </items>
      <items code="at0012">
        <items id="text">Lowest vertebra</items>
        <items id="description">The most tilted vertebra on the bottom of a spinal curve.</items>
        <items id="comment">Coding of the 'Lowest vertebra' with a terminology is preferred, where possible.</items>
      </items>
      <items code="at0015">
        <items id="text">Comment</items>
        <items id="description">Additional narrative about the cobb angle measurement, not captured in other fields.</items>
      </items>
      <items code="at0019">
        <items id="text">Apex vertebra</items>
        <items id="description">The apex of the spinal curve.</items>
        <items id="comment">Coding of the 'Apex vertebra' with a terminology is preferred, where possible.</items>
      </items>
    </term_definitions>
    <term_definitions language="nb">
      <items code="at0000">
        <items id="text">Cobbvinkel</items>
        <items id="description">En måling for å bestemme graden av krumning av ryggraden.</items>
      </items>
      <items code="at0002">
        <items id="text">Cobbvinkel</items>
        <items id="description">Målt vinkel på spinalkurven.</items>
      </items>
      <items code="at0010">
        <items id="text">Øvre vertebra</items>
        <items id="description">Den mest skråstilte ryggvirvelen på toppen av en spinalkurve.</items>
        <items id="comment">Koding av 'Øvre vertebra' med en terminologi foretrekkes, der det er mulig.</items>
      </items>
      <items code="at0012">
        <items id="text">Laveste vertebra</items>
        <items id="description">Den mest skråstilte ryggvirvelen på bunnen av en spinalkurve.</items>
        <items id="comment">Koding av 'Laveste vertebra' med en terminologi foretrekkes, der det er mulig.</items>
      </items>
      <items code="at0015">
        <items id="text">Kommentar</items>
        <items id="description">Ytterligere fritekst om målingen av cobbvinkel som ikke er registrert i andre felt.</items>
      </items>
      <items code="at0019">
        <items id="text">Apex vertebra</items>
        <items id="description">Toppen av spinalkurven.</items>
        <items id="comment">Koding av 'Apex vertebra' med en terminologi foretrekkes, der det er mulig.</items>
      </items>
    </term_definitions>
  </ontology>
