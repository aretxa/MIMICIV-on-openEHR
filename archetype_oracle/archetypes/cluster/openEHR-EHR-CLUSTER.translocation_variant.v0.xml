  <ontology>
    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Genetic translocation variant</items>
        <items id="description">Translocation variant.</items>
      </items>
      <items code="at0003">
        <items id="text">Breakpoint position 1</items>
        <items id="description">Position of first breakpoint relative to start of "Chromosome 1".</items>
      </items>
      <items code="at0004">
        <items id="text">Breakpoint position 2</items>
        <items id="description">Position of second breakpoint relative to start of "Chromosome 2".</items>
      </items>
      <items code="at0005">
        <items id="text">Strand 1</items>
        <items id="description">A value of "+" indicates that the chromosomal segment at the second breakpoint is connected to the chromosomal segment at the first breakpoint right of "Breakpoint position 1". A value of "-" indicates that the chromosomal segment at the second breakpoint is connected to the chromosomal segment of the first breakpoint left of "Breakpoint position 1".</items>
      </items>
      <items code="at0006">
        <items id="text">Strand 2</items>
        <items id="description">A value of "+" indicates that the chromosomal segment at the first breakpoint is connected to the chromosomal segment at the second breakpoint right of "Breakpoint position 2". A value of "-" indicates that the chromosomal segment at the first breakpoint is connected to the chromosomal segment of the second breakpoint left of "Breakpoint position 2".</items>
      </items>
      <items code="at0007">
        <items id="text">Reference sequence 1</items>
        <items id="description">Chromosome of first breakpoint.</items>
      </items>
      <items code="at0008">
        <items id="text">Reference sequence 2</items>
        <items id="description">Chromosome of second breakpoint.</items>
      </items>
      <items code="at0009">
        <items id="text">HGVS term</items>
        <items id="description">The description of the variant using the recommendations of the accepted HGVS nomeclature named extension ISCN.</items>
      </items>
    </term_definitions>
    <term_definitions language="de">
      <items code="at0000">
        <items id="text">Genetic translocation variant</items>
        <items id="description">Translocation variant.</items>
      </items>
      <items code="at0003">
        <items id="text">Breakpoint position 1</items>
        <items id="description">Position of first breakpoint relative to start of "Chromosome 1".</items>
      </items>
      <items code="at0004">
        <items id="text">Breakpoint position 2</items>
